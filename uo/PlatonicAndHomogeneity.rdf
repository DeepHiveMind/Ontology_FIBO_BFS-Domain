<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-plh "https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-plh="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">PlatonicAndHomogeneity</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-uo-plh;HeterogeneousThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">heterogeneous thing</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-uo-plh;HomogeneousThing"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-plh;HomogeneousThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">homogeneous thing</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-plh;PlatonicExpression">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-plh;expresses"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">platonic expression</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The expression of some Quantity in some form.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">The expression may be the explicit expression of some variable by way of some formula, measure etc., or it may be something which tracks the quantity in some way.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-plh;PlatonicQuantity">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-plh;expressedAs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">platonic quantity</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some knowable or unknowable quantity which is expressed in some way, the expression being distinct from the Quantity.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This is the common parent for things like economic value (and unknowable quantity expressed through price or valuation), and for variables which may be expressed in a number of ways such as times or prices.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-uo-plh;expressedAs">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">expressed as</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
		<rdfs:range
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<owl:inverseOf
			rdf:resource="&fibo-uo-plh;expresses"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-uo-plh;expresses">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">expresses</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<rdfs:range
			rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
	</owl:ObjectProperty>

</rdf:RDF>