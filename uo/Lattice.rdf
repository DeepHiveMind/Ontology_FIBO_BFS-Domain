<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-gao-gl "http://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/">
	<!ENTITY fibo-fnd-gaox-aag "https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectivesExt/AutonomousAgentGoals/">
	<!ENTITY fibo-fnd-inf-inf "https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/">
	<!ENTITY fibo-fnd-inf-tem "https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/">
	<!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:fibo-fnd-gao-gl="http://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"
	xmlns:fibo-fnd-gaox-aag="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectivesExt/AutonomousAgentGoals/"
	xmlns:fibo-fnd-inf-inf="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"
	xmlns:fibo-fnd-inf-tem="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/"
	xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/UO/Lattice/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Lattice</rdfs:label>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectivesExt/AutonomousAgentGoals/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;Abstract">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">abstract thing</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-uo-lat;Concrete"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;Concrete">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">concrete thing</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A thing which takes some physical form. This includes things which may be physically represented only in a computer system but where they are equivalent to some physical thing (such as a dematerialised security or a verbal contract). Sowa: &quot;Physical&quot;</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;ContinuantThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">continuant thing</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Thing which continues to exist. This is defined in opposition to the Occurrence, which is a Thing which occurs only at a specific point in time. [Reference: Sowa 2000 &quot;Continuant&quot;]</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;IndependentThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-gaox-aag;pursuesGoal"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-gao-gl;Goal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-tem;hasStatus"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-tem;Status"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">independent thing</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-uo-lat;MediatingThing"/>
		<owl:disjointWith
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A thing in its own right.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">SBVR: Fundamental Concept - plays roles in different perspectives In SBVR roles are also a kind of concept.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;MediatingThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">mediating thing</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;OccurrentThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">occurrent thing</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-uo-lat;RelativeThing">
		<rdfs:subClassOf
			rdf:resource="&owl;Thing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-lat;identity"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-lat;inContext"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">relative thing</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-uo-lat;identity">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">identity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The class of Independent Thing which is identified as this class of Relative Thing, i.e. the thing which fulfils the role or is the party etc. defined for the Independent Thing.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-uo-lat;inContext">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">in context</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">For simplicity, in some instances the definition of a relative thing is given in the context of something other than a &quot;Mediating Thing&quot; or context in the purest sense. For example a relative thing may be defined in relation to some identifiable independent thing, as for example with the native yield calculation method for a security.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The context in which the Relative Thing has the meaningful definition given to it.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>