         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has degree of control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasDegreeOfControl"
                      xmi:id="hasDegreeOfControl">
            <shortDescription>has degree of control, (partial, majority, or complete) either alone or with other parties, with respect to some activity</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has degree of control</lexicalForm>
            </RDFSlabel>
            <RDFSdomain xmi:idref="EntityControllingParty"/>
            <RDFSrange xmi:idref="DegreeOfControl"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has nature of control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasNatureOfControl"
                      xmi:id="hasNatureOfControl">
            <shortDescription>has nature or kind of control, de facto or limited de facto control</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has nature of control</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/has"/>
            <RDFSdomain xmi:idref="EntityControllingParty"/>
            <RDFSrange href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeFactoControl"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has controlling interest party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasControllingInterestParty"
                      xmi:id="hasControllingInterestParty">
            <shortDescription>indicates an entity that has the ability to exercise some control over a formal business organization </shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has controlling interest party</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/hasPartyInRole"/>
            <RDFSdomain href="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
            <RDFSrange xmi:idref="ControllingInterestParty"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has majority controlling party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasMajorityControllingParty"
                      xmi:id="hasMajorityControllingParty">
            <shortDescription>The parent of the company, if there is one.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has majority controlling party</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf xmi:idref="isControlledToSomeDegreeBy"/>
            <RDFSdomain href="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
            <RDFSrange xmi:idref="MajorityControllingParty"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has significant controlling interest party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasSignificantControllingInterestParty"
                      xmi:id="hasSignificantControllingInterestParty">
            <shortDescription>identifies an entity that owns a significant percentage of the equity in this company, but less than 50 percent</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has significant controlling interest party</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf xmi:idref="hasControllingInterestParty"/>
            <RDFSdomain href="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
            <RDFSrange xmi:idref="SignificantControllingInterestParty"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="has total owner"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasTotalOwner"
                      xmi:id="hasTotalOwner">
            <shortDescription>indicates the party that wholly controls the organization, i.e., one that owns 100 percent interest</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>has total owner</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/OwnershipParties/hasConstitutionalOwner"/>
            <RDFSdomain href="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
            <RDFSrange xmi:idref="TotalOwner"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="holds some means of"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/holdsSomeMeansOf"
                      xmi:id="holdsSomeMeansOf">
            <shortDescription>Some means of control held by the party.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>holds some means of</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/holds"/>
            <RDFSdomain xmi:idref="ControllingInterestParty"/>
            <RDFSrange href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="is based on investment equity"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/isBasedOnInvestmentEquity"
                      xmi:id="isBasedOnInvestmentEquity">
            <shortDescription>indicates investment-based de facto control, which is is based on the holding of some investment equity by some party</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>is based on investment equity</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/isConferredBy"/>
            <RDFSdomain xmi:idref="InvestmentBasedDeFactoControl"/>
            <RDFSrange href="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/OwnershipParties/InvestmentEquity"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="is controlled to some degree by"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/isControlledToSomeDegreeBy"
                      xmi:id="isControlledToSomeDegreeBy">
            <shortDescription>identifies a party that has some degree of control over a formal business organization</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>is controlled to some degree by</lexicalForm>
            </RDFSlabel>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/isControlledBy"/>
            <RDFSdomain href="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
            <RDFSrange xmi:idref="EntityControllingParty"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLDatatypeProperty" name="percentage de facto control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/percentageDeFactoControl"
                      xmi:id="percentageDeFactoControl">
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>percentage de facto control</lexicalForm>
            </RDFSlabel>
            <RDFSrange href="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/percentage"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="COCO bondholder"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/COCOBondholder"
                      xmi:id="COCOBondholder">
            <shortDescription>Holder of bank bonds which convert to equity holdings in a bank.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>COCO bondholder</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="PartyHoldingSomeOptionForControl"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="constitutional contractual control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ConstitutionalContractualControl"
                      xmi:id="ConstitutionalContractualControl">
            <shortDescription>Some control set out in some contract among the principals of an organization.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>constitutional contractual control</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="ConstitutionalDeJureControl"/>
            <RDFSsubClassOf xmi:idref="ContractualControl"/>
            <RDFSsubClassOf xmi:idref="Rest-147"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="constitutional de jure control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ConstitutionalDeJureControl"
                      xmi:id="ConstitutionalDeJureControl">
            <shortDescription>Control which is based in some instrument which has constitutional standing (is constitutive of the entity) such as voting shares, general partner equity etc.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>constitutional de jure control</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="DeJureControllingInterest"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="contractual control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ContractualControl"
                      xmi:id="ContractualControl">
            <shortDescription>Control vested in an entity by virtue of the terms in some contract between that entity and others.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>contractual control</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control"/>
            <RDFSsubClassOf xmi:idref="Rest-162"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="contractual controlling party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ContractualControllingParty"
                      xmi:id="ContractualControllingParty">
            <shortDescription>A party which exercises some control which is set out in some contract.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>contractual controlling party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="DeJureControllingInterestParty"/>
            <RDFSsubClassOf xmi:idref="Rest-171"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="controlled party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ControlledParty"
                      xmi:id="ControlledParty">
            <shortDescription>Some party which is controlled in some way and to some degree by some other party.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>controlled party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/PartyInRole"/>
            <OWLequivalentClass xmi:idref="Rest-188"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="controlling alliance"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ControllingAlliance"
                      xmi:id="ControllingAlliance">
            <shortDescription>Some group of entities which have formed some alliance or agreement in terms of which they jointly exercise control over some entity.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>controlling alliance</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/Group"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="controlling interest option holder"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ControllingInterestOptionHolder"
                      xmi:id="ControllingInterestOptionHolder">
            <shortDescription>A party which holds some option which may be exercised to confer on that party some de jure controlling interest at some time in the future and at the discretion of that holder.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>controlling interest option holder</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="PartyHoldingSomeOptionForControl"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="controlling interest party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ControllingInterestParty"
                      xmi:id="ControllingInterestParty">
            <shortDescription>Some party which holds some form of controlling interest in some other party in some context.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>controlling interest party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControllingParty"/>
            <RDFSsubClassOf xmi:idref="Rest-209"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="controlling leverage party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ControllingLeverageParty"
                      xmi:id="ControllingLeverageParty">
            <shortDescription>A party which exercises control as a result of some investment in an equity.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>controlling leverage party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="DeFactoControllingInterestParty"/>
            <RDFSsubClassOf xmi:idref="Rest-226"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="court appointed control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/CourtAppointedControl"
                      xmi:id="CourtAppointedControl">
            <shortDescription>Control conferred on some entity by act of some court, for example in the context of receivership.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>court appointed control</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control"/>
            <RDFSsubClassOf xmi:idref="Rest-242"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="de facto controlling interest party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DeFactoControllingInterestParty"
                      xmi:id="DeFactoControllingInterestParty">
            <shortDescription>A party which exercises some control over an entity other than via some constitutional instrument such as shares.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>de facto controlling interest party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="ControllingInterestParty"/>
            <RDFSsubClassOf xmi:idref="Rest-252"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="degree of control"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DegreeOfControl"
                      xmi:id="DegreeOfControl">
            <shortDescription>the degree of control one entity has over another</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>degree of control</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="Rest-267"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="de jure controlling interest"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DeJureControllingInterest"
                      xmi:id="DeJureControllingInterest">
            <shortDescription>controlling interest that is formalized by some formal legal construct</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>de jure controlling interest</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeJureControl"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="de jure controlling interest party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DeJureControllingInterestParty"
                      xmi:id="DeJureControllingInterestParty">
            <shortDescription>An entity that may exercise de jure controlling interest</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>de jure controlling interest party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="ControllingInterestParty"/>
            <RDFSsubClassOf xmi:idref="Rest-280"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="entity controlling party"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/EntityControllingParty"
                      xmi:id="EntityControllingParty">
            <shortDescription>Some party which is able to direct the activities of some business entity.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>entity controlling party</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf href="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControllingParty"/>
            <RDFSsubClassOf xmi:idref="Rest-298"/>
         </owlUniverse>
