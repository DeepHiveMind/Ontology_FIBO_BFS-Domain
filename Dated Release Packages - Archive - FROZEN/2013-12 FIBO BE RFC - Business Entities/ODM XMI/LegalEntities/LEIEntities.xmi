               <resource xmi:idref="LegalEntityIdentifier"/>
            </RDFsubject>
            <RDFpredicate href="http://www.w3.org/2004/02/skos/core#definition"/>
            <RDFobject xmi:type="odm:PlainLiteral">
               <lexicalForm>A code which uniquely identifies a formal business organization and is allocated to it as a result of it being recognized as being a contractually capable entity (ISO 17442 Legal Entity).</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
