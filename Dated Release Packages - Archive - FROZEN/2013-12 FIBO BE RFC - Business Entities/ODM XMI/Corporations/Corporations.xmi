            <RDFobject xmi:type="odm:PlainLiteral">
               <lexicalForm>has subscribed share capital</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
            <RDFsubject xmi:type="odm:URIReferenceNode">
               <resource xmi:idref="hasIssuedCapital"/>
            </RDFsubject>
            <RDFpredicate href="http://www.w3.org/2004/02/skos/core#definition"/>
            <RDFobject xmi:type="odm:PlainLiteral">
               <lexicalForm>The total of a companys shares that are held by shareholders. A company can, at any time, issue new shares up to the full amount of authorized share capital.</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
