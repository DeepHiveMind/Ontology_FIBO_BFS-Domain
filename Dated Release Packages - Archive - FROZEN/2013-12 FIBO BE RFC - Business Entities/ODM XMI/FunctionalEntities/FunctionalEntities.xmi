                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/FunctionalEntities/FunctionalEntities/Commerce"
                      xmi:id="Commerce">
            <shortDescription>The activity of buying and selling goods.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>commerce</lexicalForm>
            </RDFSlabel>
            <RDFSsubClassOf xmi:idref="CommercialActivity"/>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="commercial activity"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/FunctionalEntities/FunctionalEntities/CommercialActivity"
                      xmi:id="CommercialActivity">
            <shortDescription>The context of carrying out trade and other comercial i.e. for-profit activities.</shortDescription>
            <RDFSlabel xmi:type="odm:PlainLiteral">
               <lexicalForm>commercial activity</lexicalForm>
            </RDFSlabel>
         </owlUniverse>
         <owlUniverse xmi:type="odm:OWLClass" name="cooperative society"
