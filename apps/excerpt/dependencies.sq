prefix afn: <http://jena.apache.org/ARQ/function#>
prefix fn: <http://www.w3.org/2005/xpath-functions#> 
prefix owl: <http://www.w3.org/2002/07/owl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
prefix skos: <http://www.w3.org/2004/02/skos/core#> 
prefix sp: <http://spinrdf.org/sp#> 
prefix spin: <http://spinrdf.org/spin#> 
prefix spl: <http://spinrdf.org/spl#> 
prefix xsd: <http://www.w3.org/2001/XMLSchema#> 


# Find all the ontologies that IRSwaps owl:imports
# Find all the classes,
# Look for the classes whose namespace matches the namespace of one of these ontologies. 

SELECT ?c
WHERE {<http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/> owl:imports+ ?o .
?c a owl:Class . 
FILTER (STRSTARTS (xsd:string (?c), xsd:string (?o)))
}
