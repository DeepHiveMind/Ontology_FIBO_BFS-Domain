    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/ExerciseAction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/MandatoryCorporateEventProcess">
    <skos:definition xml:lang="en">Mandatory corporate event: Action requires an action by the shareholder Corporate action event initiated by the corporation. Participation by shareholders is mandatory (for those relating to shares).</skos:definition>
    <rdfs:label xml:lang="en">mandatory corporate event process</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/MandatoryEventStart"/>
