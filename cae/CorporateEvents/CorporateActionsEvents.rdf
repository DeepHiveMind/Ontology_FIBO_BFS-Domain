    <rdfs:label xml:lang="en">capital distribution notification</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/IssuerCorporateActionNotificationMessage"/>
