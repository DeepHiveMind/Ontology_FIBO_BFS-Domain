    <rdfs:label xml:lang="en">third party action announcement</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/PartialCall">
    <rdfs:label xml:lang="en">partial call action</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/CallAction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/ExtraordinaryGeneralMeeting">
    <skos:definition xml:lang="en">Extraordinary General Meeting</skos:definition>
    <rdfs:label xml:lang="en">extraordinary general meeting</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CAE/CorporateEvents/CorporateActionsEvents/Meeting"/>
