
(:query-type :sparql :query-name "" :query-string
 "# select the most accurate or specialized inferred type for a party in role individual

select ?c {

tst-bcoac:BigBankSecuritiesIncPIR a ?c .
  filter(?c != owl:NamedIndividual) .
   filter( not exists {
    ?other rdfs:subClassOf ?c .
    tst-bcoac:BigBankSecuritiesIncPIR a ?other .
      } )

 } order by ?c
    
")
