
(:query-type :sparql :query-name "" :query-string
 "SELECT distinct ?a ?b  ?c ?rl1 ?d  ?rl2 
WHERE {
 
 ?a fnd-oac-ctl:isControlledBy ?b .
 ?a  fnd-pty-rl:isPartyInRole ?c .
 ?c  fnd-pty-rl:hasRole    ?rl1.
 ?b  fnd-pty-rl:isPartyInRole ?d .
 ?d  fnd-pty-rl:hasRole    ?rl2.
}
order by ?a ?b ?c ?rl1 ?d ?rl2")
