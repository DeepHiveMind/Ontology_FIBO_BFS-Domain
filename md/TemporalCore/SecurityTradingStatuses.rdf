<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl1="http://www.w3.org/2002/07/owl/"
    xmlns:fibo-md-tem-trs="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/"
    xmlns:fibo-md-tem-tem="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-sec-sec-lisx="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListingsExt/"
    xmlns:fibo-fnd-inf-inf="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"
    xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
    xmlns:fibo-fnd-inf-tem="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListingsExt/"/>
    <rdfs:label xml:lang="en">SecurityTradingStatuses</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/PastStatus">
    <rdfs:label xml:lang="en">past status</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Past"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/statusHasTense"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/StatusCodeValue"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Worthless">
    <fibo-fnd-utl-av:explanatoryNote xml:lang="en">e.g. bankruptcy hearings - might result in this being said.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition xml:lang="en">Announcement by the regulator that the security has become worthless.</skos:definition>
    <rdfs:label xml:lang="en">worthless</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security">
            <owl:equivalentClass>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Security"/>
            </owl:equivalentClass>
          </rdf:Description>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/describes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Security">
    <rdfs:label xml:lang="en">security</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/TradingHalted">
    <rdfs:label xml:lang="en">trading halted</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Inactive">
    <rdfs:label xml:lang="en">inactive</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Issued">
    <skos:definition xml:lang="en">The security has been issued into the secondary market.</skos:definition>
    <rdfs:label xml:lang="en">issued</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ActivelyTrading">
    <skos:definition xml:lang="en">Security is actively traded on the exchange or trading facility</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/TradingHalted"/>
    <rdfs:label xml:lang="en">actively trading</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityListing">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListings/SecurityListing"/>
    <rdfs:label xml:lang="en">security listing</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus">
    <skos:definition xml:lang="en">The trading status of a security on a given trading exchange.</skos:definition>
    <rdfs:label xml:lang="en">exchange security trading status</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus">
    <skos:definition xml:lang="en">The status of a security within its lifecycle.</skos:definition>
    <rdfs:label xml:lang="en">security lifecycle status</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/CurrentStatus">
    <skos:definition xml:lang="en">The status at the present time.</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/PastStatus"/>
    <rdfs:label xml:lang="en">current status</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Present"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/statusHasTense"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/StatusCodeValue"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/WhenIssued">
    <skos:definition xml:lang="en">When Issued or Gray Market trading is when a security is traded ahead of the date at which it is to be issued.</skos:definition>
    <rdfs:label xml:lang="en">when issued</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/WhenDistributed">
    <skos:definition xml:lang="en">Used to refer to a security that trades after the date of issue but before the time at which the certificates are delivered</skos:definition>
    <rdfs:label xml:lang="en">when distributed</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus">
    <skos:editorialNote xml:lang="en">Exchange Traded Security trading status is now a separate term, covering trading suspension on an exchange, so that does not form part of this term.</skos:editorialNote>
    <skos:definition xml:lang="en">The status of the security in terms of whether it is trading or not, and any special considerations relating to trading.</skos:definition>
    <rdfs:label xml:lang="en">security trading status</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/Status"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Active">
    <skos:definition xml:lang="en">Security is actively traded</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Inactive"/>
    <rdfs:label xml:lang="en">active</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/hasStatus">
    <skos:definition xml:lang="en">The trading status of this listing i.e. this security on this exchange.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityListing"/>
    <rdfs:label xml:lang="en">has listing status</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/hasStatus"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/hasTradingStatus">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Security"/>
    <rdfs:label xml:lang="en">has trading status</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/hasStatus"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/status">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
    <rdfs:label xml:lang="en">status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/delisted">
    <skos:definition xml:lang="en">Whether the security is currently delisted on that exchange or trading facility.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus"/>
    <rdfs:label xml:lang="en">delisted</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/describes">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Worthless"/>
    <rdfs:label xml:lang="en">describes</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/isOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/worthless">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/Worthless"/>
    <rdfs:label xml:lang="en">worthless</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/statusDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/PastStatus"/>
    <rdfs:label xml:lang="en">status date</rdfs:label>
    <skos:definition xml:lang="en">Date at which the status of the security is taken.</skos:definition>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/tense.1">
    <rdfs:label xml:lang="en">tense</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

