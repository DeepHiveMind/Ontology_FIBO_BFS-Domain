        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/DerivativesTemporal/ETOptionsTemporal/ETOptionPremium"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >reflects</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/purportsToReflect"/>
