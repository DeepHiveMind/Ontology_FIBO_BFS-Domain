    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >defined by</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/DerivativesTemporal/ETOptionsTemporal/refersTo">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/MD/DerivativesTemporal/ETOptionsTemporal/OptionTheoreticalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/MD/DerivativesTemporal/ETOptionsTemporal/OptionsTheta"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >refers to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/MD/DerivativesTemporal/ETOptionsTemporal/expressedAs">
