bubu<rdfs:subClassOf rdf:resource="&fibo-md-dbtx-py;LoanPoolAnalytic"/>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-md-dbtx-py;derivedFrom.1"/>
