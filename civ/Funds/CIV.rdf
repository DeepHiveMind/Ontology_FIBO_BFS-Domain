bubu<rdfs:subClassOf rdf:resource="&fibo-civ-fnd-civ;StakeInFund"/>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-civ-fnd-civ;definesRelationshipWith"/>
