<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl1="http://www.w3.org/2002/07/owl/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-civ-fnd-civ="https://spec.edmcouncil.org/fibo/CIV/Funds/CIV/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-fnd-inf-inf="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-civ-fnd-clx="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
  xml:base="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/CIV/Funds/CIV/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >CIVClassification</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/Fund">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fund</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/FundClassificationScheme">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A published scheme for the category of a fund according to the asset class.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fund classification scheme</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/FundClassification"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/defines.1"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/ClassificationScheme"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/FundClassification">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The category of the fund according to the asset class. This is a published code by which the Fund is classified. Further notes: Could be EFCF codes. This is the way in which "Type of fund" is articulated.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fund classification</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/CIV/Funds/CIV/Fund">
            <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/CIV/Funds/CIVClassification/Fund"/>
          </rdf:Description>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/classifies"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/Classification"/>
  </owl:Class>
</rdf:RDF>

