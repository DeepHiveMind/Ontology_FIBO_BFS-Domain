    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Asset -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Asset">
        <rdfs:label>asset</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/isPlayedBy"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwnedBy"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Owner"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasAcquisitionDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A thing held by some party and having some value.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Owner -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Owner">
        <rdfs:label>owner</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/owns"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A party in the ownership role; one that owns something. The thing owned is an Asset to that Party.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Ownership -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Ownership">
        <rdfs:label>ownership</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/DatesAndTimes/BusinessDates/holdsDuring"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/DatesAndTimes/FinancialDates/DatePeriod"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/hasOwningParty"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/PartyInRole"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/hasOwnedThing"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ownership is the context in which some Party is said to own some Independent Thing. The Party is defined as such due to its being the owning party to that Thing.</skos:definition>
    </owl:Class>
    


