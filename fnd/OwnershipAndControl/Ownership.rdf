        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">identifies the party that owns the asset</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Asset"/>
