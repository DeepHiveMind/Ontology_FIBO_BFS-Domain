    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/OwnershipAndControl">
        <rdfs:label>ownership and control</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control"/>
