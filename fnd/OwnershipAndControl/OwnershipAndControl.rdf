    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/isOwnedAndControlledBy -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/isOwnedAndControlledBy">
        <rdfs:label>is owned and controlled by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a relationship between some thing and the party that owns, influences, manages and directs it</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/IndependentParty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/ownsAndControls -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/ownsAndControls">
        <rdfs:label>owns and controls</rdfs:label>
        <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string">basic rule: if  x controls y and x owns y then x owns and controls y
SWRL rule: controls(?x, ?y), owns(?x, ?y) -&gt; ownsAndControls(?x, ?y)</skos:editorialNote>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">directs and exercises authoritative or dominating influence over some thing that is also owned</skos:definition>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/OwnershipAndControl/isOwnedAndControlledBy"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/IndependentParty"/>
    </owl:ObjectProperty>
    


