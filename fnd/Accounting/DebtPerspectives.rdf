      </rdf:Description>
    </owl:disjointWith>
    <owl:disjointWith>
      <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance">
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
      </rdf:Description>
    </owl:disjointWith>
  </rdf:Description>
</rdf:RDF>

