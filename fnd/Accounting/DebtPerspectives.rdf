    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >DebtPerspectives</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Money which is owed by one party (the Debtor) to another party (the Creditor) at some point in time, as seen from the perspective of the Debtor.</skos:definition>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/seenFromPerspectiveOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/isDebtTo"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >debt side</rdfs:label>
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This is an amount that varies over time, in the form of a debt balance. This is distinct from "Debt" such as a bond or other debt instrument, which is a specific debt amount advanced to the borrower at a given point of time; the balance of the latter at any point in time is the debt owed by the borrower (as Debtor) to the Lender (as Creditor).</fibo-fnd-utl-av:explanatoryNote>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/isPositionInRespectOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/OwingSide"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/Liability"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/SocialConstructs/ConstructAspects/Claim"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide">
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >credit side</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
