      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/isPositionInRespectOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/hasOtherViewpoint"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/SocialConstructs/ConstructAspects/Claim"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/OwingSide"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/OwingSide">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >owing side</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/hasOtherViewpoint">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has other viewpoint</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasOtherSide"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/isDebtTo">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is debt to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/isPositionInRespectOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/OwingSide"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is position in respect of</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/isAspectOf"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/AccountReceivableBalance">
