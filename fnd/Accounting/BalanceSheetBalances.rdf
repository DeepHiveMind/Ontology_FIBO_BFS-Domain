<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-be-le-fbo="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-acc-dbpx="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:fibo-fnd-acc-acc="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"
    xmlns:fibo-fnd-acc-bal="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-fnd-acc-assx="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/"
    xmlns:fibo-fnd-acc-aeq="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/"
    xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
    xmlns:fibo-ext-snap-par-wt="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >BalanceSheetBalances</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetFeature">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet feature</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetAssetBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A balance sheet entry representing some asset of the entity.</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/AccountReceivableBalance"/>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance"/>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/OwnersEquity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetLiabilityBalance"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet asset balance</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetBalance">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet balance</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetFeature"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetTotal">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet total</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetFeature"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A balance of cash. This is identified as an amount of cash which is denominated with a currency.</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
    <owl:disjointWith>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >cash balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/AmountOfMoney"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/representsMonetaryAmount"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/CashCurrency"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/representsAmountOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/LedgerBalance"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/OwnersEquity">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >owners equity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/VostroBalance">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >vostro balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/Balance"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetLiabilityBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A balance sheet entry representing some asset of the entity.</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/AccruedLiability"/>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/AccountPayableBalance"/>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/DebtSide"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/OwnersEquity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet liability balance</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Balance of stock in trade.</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >stock balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/StockAmount"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/represents"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/StockInTrade"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isQuantityOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/PhysicalAsset"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/NostroBalance">
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/VostroBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >nostro balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/Balance"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/AccountReceivableBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Balance of accounts receivable</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/CreditSide"/>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >account receivable balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/AccountReceivable"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isBalanceOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/LedgerBalance"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetEntry">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Some entry on the balance sheet of some business entity.</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetAssetBalance"/>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetLiabilityBalance"/>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/OwnersEquity"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance sheet entry</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/AccountPayableBalance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Balance of accounts receivable</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >account payable balance</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/AccountPayable"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isBalanceOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/LedgerBalance"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/Balance">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isQuantityOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/StockInTrade"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is quantity of</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/appliesTo">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/Interval"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetEntry"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >applies to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isBalanceOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/Account"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/Balance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is balance of</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/representsMonetaryAmount">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/AmountOfMoney"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >represents monetary amount</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/isFor">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetEntry"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is for</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/balance">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/BalanceSheetBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/represents">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/StockAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/StockBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >represents</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/representsAmountOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/CashCurrency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/CashBalance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >represents amount of</rdfs:label>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/OwnersEquity">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/OwnersEquity"/>
  </rdf:Description>
</rdf:RDF>

