    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/DebtInterestPaymentTermsSet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >accrual basis</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/principalRepaymentDate">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The date when the principal of the debt or loan is to have been repaid in full. Depending on the terms of the debt, or the terms of a debt security in the case of secufritized debt, this may be the date of a single payment of the debt principal or o fthe completion of scheduled partial redemption payments.</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/DebtPrincipalRepaymentTermsSet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >principal repayment date</rdfs:label>
  </owl:DatatypeProperty>
