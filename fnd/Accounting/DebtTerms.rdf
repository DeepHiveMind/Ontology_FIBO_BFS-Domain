	xmlns:LegalPersons="https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"
	xmlns:MethodsAndRules="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/"
	xmlns:Ownership="https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/"
	xmlns:Parties="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:PartyRoleContext="https://spec.edmcouncil.org/fibo/FND/PartiesExt/PartyRoleContext/"
	xmlns:TemporalConcepts="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/"
