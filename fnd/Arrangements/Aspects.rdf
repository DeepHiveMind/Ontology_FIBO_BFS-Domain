<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-arrx-asp "https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/">
	<!ENTITY fibo-fnd-pty-pty "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/"
	xmlns:fibo-fnd-arrx-asp="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Aspects</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-asp;Aspect">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REAClaimsEvents/asSeenBy"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REAClaimsEvents/AspectViewingParty"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">aspect</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Something defined as from some perspective. Further notes: Develop this for rights and obligation perspectives as well.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-asp;Form">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">form</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The form which something takes.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-asp;Side">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">side</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An aspect of something that may be seen from one of two aspects, as seen from viewers at two viewpoints of that thing.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This may be a concrete (two dimensional) thing, or an abstract thing which can be seen from two sides.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-asp;hasAlternativeViewpoint">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has alternative viewpoint</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-asp;hasOtherSide">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has other side</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-asp;isAspectOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is aspect of</rdfs:label>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-asp;seenFromPerspectiveOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">seen from perspective of</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
	</owl:ObjectProperty>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/REAClaimsEvents/AspectViewingParty">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
	</rdf:Description>

</rdf:RDF>