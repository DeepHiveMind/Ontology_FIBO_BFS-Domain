<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl1="http://www.w3.org/2002/07/owl/"
    xmlns:fibo-fnd-arrx-asp="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core/"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Aspects</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >aspect</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/REAClaimsEvents/AspectViewingParty">
            <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/PartyInRole"/>
          </rdf:Description>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REAClaimsEvents/asSeenBy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/isAspectOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasAlternativeViewpoint"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Something defined as from some perspective. Further notes: Develop this for rights and obligation perspectives as well.</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Form">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >form</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The form which something takes.</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This may be a concrete (two dimensional) thing, or an abstract thing which can be seen from two sides.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >An aspect of something that may be seen from one of two aspects, as seen from viewers at two viewpoints of that thing.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >side</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/PartyInRole"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/seenFromPerspectiveOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasOtherSide"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasOtherSide"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasAlternativeViewpoint">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has alternative viewpoint</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/isAspectOf">
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Aspect"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is aspect of</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasOtherSide">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has other side</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/hasAlternativeViewpoint"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/seenFromPerspectiveOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/PartyInRole"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Aspects/Side"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >seen from perspective of</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

