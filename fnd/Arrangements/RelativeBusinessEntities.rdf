<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-be-fct-fct "https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/">
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-arrx-rbe "https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/"
	xmlns:fibo-be-fct-fct="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-arrx-rbe="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">RelativeBusinessEntities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-be-fct-fct;FunctionalBusinessEntity">
		<owl:equivalentClass
			rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-rbe;BusinessContext">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">business context</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-rbe;RelativeBusinessEntity">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-fct-fct;FunctionalEntity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">relative business entity</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A business entity defined in some relative sense, for example by reference to its role in some process or its overall role or function in the marketplace or in the world as a whole.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-rbe;relativeBusinessEntityDefinedInContextOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">relative business entity defined in context of</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The context in which the Relative Business Entity is defined and in which some entity performs the defined role.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-rbe;takesEntityForm">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">takes entity form</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The form which a Relative Business Entity takes. This may be an Autonomous Agent.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>