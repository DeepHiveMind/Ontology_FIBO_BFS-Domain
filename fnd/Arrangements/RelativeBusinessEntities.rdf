<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl1="http://www.w3.org/2002/07/owl/"
    xmlns:fibo-fnd-arrx-rbe="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/"
    xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
    xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-be-fct-fct="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >RelativeBusinessEntities</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/BusinessContext">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >business context</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/RelativeBusinessEntity">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >relative business entity</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/takesEntityForm"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/BusinessContext"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/relativeBusinessEntityDefinedInContextOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A business entity defined in some relative sense, for example by reference to its role in some process or its overall role or function in the marketplace or in the world as a whole.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/FunctionalEntity"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/relativeBusinessEntityDefinedInContextOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/BusinessContext"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/RelativeBusinessEntity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >relative business entity defined in context of</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The context in which the Relative Business Entity is defined and in which some entity performs the defined role.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/takesEntityForm">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The form which a Relative Business Entity takes. This may be an Autonomous Agent.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/RelativeBusinessEntity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >takes entity form</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/hasIdentity"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/FunctionalBusinessEntity">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/FunctionalBusinessEntity"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/RelativeBusinessEntities/RelativeBusinessEntity"/>
  </rdf:Description>
</rdf:RDF>

