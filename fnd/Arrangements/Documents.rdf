    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/Document -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/Document">
        <rdfs:label>document</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">something, such as a recording or a photograph, or a writing that can be used to furnish evidence or information</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/LegalDocument -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/LegalDocument">
        <rdfs:label>legal document</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/Document"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a written or printed paper that bears the original, official, or legal form of something and can be used to furnish decisive evidence or information</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/ReferenceDocument -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Arrangements/Documents/ReferenceDocument">
        <rdfs:label>reference document</rdfs:label>
