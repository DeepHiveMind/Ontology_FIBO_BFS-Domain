<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-arrx-pq "https://spec.edmcouncil.org/fibo/FND/Arrangements/PlatonicQuantities/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY fibo-uo-plh "https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/PlatonicQuantities/"
	xmlns:fibo-fnd-arrx-pq="https://spec.edmcouncil.org/fibo/FND/Arrangements/PlatonicQuantities/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:fibo-uo-plh="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/PlatonicQuantities/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">PlatonicQuantities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;AbstractQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">abstract quantity</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some abstract notion such as direction, color or shape.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Description">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">description</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some representation of some abstract quantity, in words, symbols or the like.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Direction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;AbstractQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">direction</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;KnowableQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-plh;expressedAs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;Specification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">knowable quantity</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-arrx-pq;UnknowableQuantity"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some quantity which is directly apprehensible to senses or to measurement and may be specified in some Specification.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Level">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;AbstractQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">level</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An abstraction identifying how high or low some aspect of something is or is perceived to be.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Meaning">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;UnknowableQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">meaning</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The referent of some meaningful symbol.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">We do not attempt to define meaning itself here. The relationshpoi between Symbol and Meaning is the Peircean semiotic relationship.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;ObjectiveProbability">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;UnknowableQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">objective probability</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The actual probabilty of an event or situation. This is approximated to by various probabillity measures accoding to the available knowledge, but is itself unchanged.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Openness">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;AbstractQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">openness</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Whether something is open or closed. Further notes: What this means precisely depends on how the concept of openness or closedness is being applied, for example to a fund. Apples for example to open ended versus closed funds, as well as (in principle) boxes and the like.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;ProbabilityMeasure">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-uo-plh;expresses"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;ObjectiveProbability"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">probability measure</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The measure of an unknown objective probabilty, this will come closer to the actual but unknown objective probability acoording to the information available.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Specification">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-pq;specifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;KnowableQuantity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">specification</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">That which specifies something.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;SpecifiedVariableQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;KnowableQuantity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-pq;specifiedAs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;VariableSpecification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">specified variable quantity</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;Symbol">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicExpression"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-pq;symbolizes"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;Meaning"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">symbol</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A symbol which is held to express some meaning.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;UnknowableQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-plh;PlatonicQuantity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">unknowable quantity</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-pq;VariableSpecification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-pq;Specification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-pq;mutableTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;VariableSpecification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-pq;specifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-pq;SpecifiedVariableQuantity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">variable specification</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">That which specifies some Variable.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-pq;mutableTo">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">mutable to</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-pq;VariableSpecification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-pq;VariableSpecification"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Each way in which the variable quantity may be expressed, is mutable to another equivalent way in which that quantity may also be expressed while remaining the same quantity.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-pq;specifiedAs">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">specified as</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-pq;SpecifiedVariableQuantity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-pq;VariableSpecification"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-pq;specifies">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-uo-plh;expresses"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">specifies</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-pq;Specification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-pq;KnowableQuantity"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The thing which is specified by the Specification.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This may be a Continuant Thing or an Occurrent Thing. It is always an Independent Thing on the assumption that the relative nature of a Relative Thing is not specified but rather the independent thing is specified in whatever relative context the specification is applied. Further, since Specification is a kind of Expression, this is already some kind of Quantity, in this case some Knowable Quantity.</fibo-fnd-utl-av:explanatoryNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-pq;symbolizes">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-uo-plh;expresses"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">symbolizes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-pq;Symbol"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-pq;Meaning"/>
	</owl:ObjectProperty>

</rdf:RDF>