bubu<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
bubu<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/FND/TransactionsExt/SupplyTransactions/"/>
