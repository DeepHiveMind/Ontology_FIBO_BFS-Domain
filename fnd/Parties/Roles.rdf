    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/isPlayedBy -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/isPlayedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label>is played by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates the actor (the independent thing) that performs a role</skos:definition>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole"/>
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/playsRole"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasIdentity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/playsRole -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/playsRole">
        <rdfs:label>plays role</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates the role that an actor (independent thing) performs</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole"/>
    </owl:ObjectProperty>
    


