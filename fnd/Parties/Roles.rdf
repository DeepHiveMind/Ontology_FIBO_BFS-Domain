    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/AgentInRole -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/AgentInRole">
        <rdfs:label>agent in role</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasIdentity"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An agent-in-role is a relative concept that ties an autonomous agent to a role they are playing in a given situational context.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OMG Property and Casualty Information Models, dtc/12-01-04, Annex A, Glossary of Data Model Terms and Definitions</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/ThingInRole">
        <rdfs:label>thing in role</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasIdentity"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A thing-in-role is a relative concept that ties some thing to a role it plays in a given situational context.</skos:definition>
    </owl:Class>
    


