<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-aapx-nhe "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeopleExt/NonHumanEntities/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeopleExt/NonHumanEntities/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-aapx-nhe="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeopleExt/NonHumanEntities/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeopleExt/NonHumanEntities/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">NonHumanEntities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-aapx-nhe;MechanicalRobot">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aapx-nhe;Robot"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">mechanical robot</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some robot which has physical extent.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-aapx-nhe;NonHumanAnimal">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">non human animal</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some animal other than a member of the species /homo/ /sapiens/</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Included for completeness and scope of the model. Completes the range of entities that come under the grouping &quot;Autonomous Entity&quot;. Note that there are legal moves around the concept of &quot;Non human persons&quot; (self aware animals such as dolphins). These are not covered in the present model. It would be necessary to add these only if we were to extend this model to describe the rights extended to persons which may include this category.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-aapx-nhe;Robot">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">robot</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Any self actualizing artificial entity.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-aapx-nhe;SoftwareAgent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aapx-nhe;Robot"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">software agent</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some self-actualizing routine or set of routine which does not have physical extent.</skos:definition>
	</owl:Class>

</rdf:RDF>