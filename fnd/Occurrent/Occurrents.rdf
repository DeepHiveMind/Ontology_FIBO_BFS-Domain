	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Occurrents</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/FND/Arrangements/Quantities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectivesExt/DesiredResults/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;ContinuousOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-occ;occursOverInterval"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ext-snap-par-wt;Interval"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">continuous occurrent</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;DiscreteOccurrent"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;CulminatingOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">culminating occurrent</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;NonCulminatingOccurrent"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Definition An occurrent which has or can come to completion. Editorial Note:</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;DescriptiveOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">descriptive occurrent</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;PrescriptiveOccurrent"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;DeterministicOccurrenrt">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">deterministic occurrenrt</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;NonDeterministicOccurrent"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;DiscreteOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-occ;hasTemporalGranularity"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-tim-tim;TemporalUnit"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-occ;occursAtInstant"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ext-snap-par-wt;Instant"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">discrete occurrent</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Definition: An occurrent with no interval Explanatory Note: This is an occurrent which is described in terms of it being instant; however, in reality such a description depends upon the definition of some temporal unit in terms of which this is considered a unit, such as a day or a second/ Editorial note: This allows us to talk about concepts which are defined with reference to an instant, where that instant may be a day or an hour or some other unit. The nature of many concepts in financial services is that they frame something as an instant at one or another scale, for example the day on which some instrument. Explanatory Note: Whenever we call something an instant we say so with reference to some unit of measure.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;End">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;PossibleState"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">end</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Some state which is or may be sought or attained by something Definition Origin Placeholder, made up for now. BMI</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;FunctionalOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">functional occurrent</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;NonFunctionalOccurrent"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Occurrent which is defined in relation to some function</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;HistoricOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;DescriptiveOccurrent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">historic occurrent</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-occ-occ;HypotheticalOccurrent"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;HypotheticalOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;DescriptiveOccurrent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">hypothetical occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;IntendedOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;FunctionalOccurrent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-occ;isIntendedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-occ;isPursuantTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-gaox-dr;DesiredResult"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">intended occurrent</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Definition: A functional occurrent which is intended by some agent.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;InterestAccrual">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;CulminatingOccurrent"/>
