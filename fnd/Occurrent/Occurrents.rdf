		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">prescriptive occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;StateOfAThing">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;Concrete"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:subClassOf
