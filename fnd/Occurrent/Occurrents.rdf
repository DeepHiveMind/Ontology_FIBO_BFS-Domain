			xml:lang="en">culminates in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-occ;CulminatingOccurrent"/>
		<rdfs:range
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-occ;hasGranularity">
