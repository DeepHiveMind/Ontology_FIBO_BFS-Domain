		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">plan</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;PossibleState">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;Abstract"/>
