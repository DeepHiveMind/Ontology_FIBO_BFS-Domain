			rdf:datatype="&rdf;langString"
			xml:lang="en">non deterministic occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;NonFunctionalOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">non functional occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;NonIntendedOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;NonFunctionalOccurrent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">non intended occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;OngoingPurposefulOccurrent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;ContinuousOccurrent"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;IntendedOccurrent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">ongoing purposeful occurrent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-occ;Plan">
		<rdfs:subClassOf
