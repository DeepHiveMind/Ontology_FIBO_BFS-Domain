		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-occ-occ;hasGranularity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has temporal granularity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-occ;DiscreteOccurrent"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-tim-tim;TemporalUnit"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-occ;isIntendedBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is intended by</rdfs:label>
