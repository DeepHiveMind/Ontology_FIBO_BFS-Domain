<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-gao-gl "https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/">
	<!ENTITY fibo-fnd-occ-ev "https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
	<!ENTITY fibo-fnd-occ-lif "https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/">
	<!ENTITY fibo-fnd-occ-proc "https://spec.edmcouncil.org/fibo/FND/Occurrent/Processes/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/"
	xmlns:fibo-fnd-gao-gl="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"
	xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-fnd-occ-lif="https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/"
	xmlns:fibo-fnd-occ-proc="https://spec.edmcouncil.org/fibo/FND/Occurrent/Processes/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Lifecycles</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Processes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-lif;Lifecycle">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;Abstract"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-lif;includes"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-ev;Event"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-lif;isLifecycleOf"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">lifecycle</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-lif;Milestone">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-occ-proc;BusinessProcessDefinition"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-lif;signalsCompletion"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-lif;realizes"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-gao-gl;Goal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">milestone</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An event with a date and some achievement of some stated Goal.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-lif;includes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">includes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-lif;Lifecycle"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-lif;isLifecycleOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is lifecycle of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-lif;Lifecycle"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-lif;realizes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">realizes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-lif;Milestone"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-gao-gl;Goal"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-lif;signalsCompletion">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">signals completion</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-lif;Milestone"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-proc;BusinessProcessDefinition"/>
	</owl:ObjectProperty>

</rdf:RDF>