	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;BusinessProcessDefinition">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-bp-prc-fcp;context"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">business process definition</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A set of activities carried out within some Business Context and in pursuance of some Business Goal as indicated by that context. A Business Activity consists of a number of Events and Activities, which have dependencies and activity flow between them.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;ContractualCommitment">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contractual commitment</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;Decision">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">decision</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Decision as understood in business process flow diagrams and models. This has a &quot;Decision Point&quot; leading into it, and two or more outcomes.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;ProcessActivity">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">process activity</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An occurrence or activity defined in a formal Process, for example Process Step (Activity), Start and End.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;ProcessActor">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">process actor</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-proc;ProcessActorContactInformation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-inf;Information"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">process actor contact information</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Contact details as required for a contact person within the Legal Entity that is the Actor in a Business Process.</skos:definition>
	</owl:Class>
	
	<owl:Class
