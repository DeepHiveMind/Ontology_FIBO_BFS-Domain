<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-arrx-agt "https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/">
	<!ENTITY fibo-fnd-gao-gl "https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/">
	<!ENTITY fibo-fnd-occ-act "https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/">
	<!ENTITY fibo-fnd-occ-ev "https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
	<!ENTITY fibo-fnd-pty-pty "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-arrx-agt="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/"
	xmlns:fibo-fnd-gao-gl="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"
	xmlns:fibo-fnd-occ-act="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Activities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/GoalsAndObjectives/Goals/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-act;Activity">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;endsWithSome"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-ev;Event"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;includesSome"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-ev;Event"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;requiresResource"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-uo-lat;ContinuantThing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;startsWithSome"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-ev;Event"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-ev;agency"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-act;Actor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">activity</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An Activity is an Event with some human agency. It is defined as something with a Time, Place, Actor and Resource(s). Also known as Event Context or just Context (see e.g DOI ontology).</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-act;Actor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-agt;EnablingAgent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;ActorIdentity"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;identifiedAsActor"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-act;Actor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;motive"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-gao-gl;Goal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-act;role"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-act;Activity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">actor</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An Actor is an independent entity defined in relation to some role in some process or event (or generally, in any Occurrence). Note: The Role itself is shown as a relationship fact.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;ActorIdentity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">actor identity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;becomes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">becomes party</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An Actor in a Process may become a Party to a contract or other participatory context, as part of the outcome of the Process.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;endsWithSome">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-occ-act;includesSome"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">ends with some</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;identifiedAsActor">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">identified as actor</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The possible identity of the Actor is framed in terms of another Actor.</skos:definition>
		<fibo-fnd-utl-av:usageNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This would typically be used to say what kind of thing can be an actor at a given point in a process or activity, in terms of another activity or process in which someone is already an actor. This is an Actor to Actor identity relation.</fibo-fnd-utl-av:usageNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;includesSome">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">includes some</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;motive">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">motive</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-gao-gl;Goal"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;requiresResource">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">requires resource</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
		<rdfs:range
			rdf:resource="&fibo-uo-lat;ContinuantThing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;role">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">role</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-act;startsWithSome">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-occ-act;includesSome"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">starts with some</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
	</owl:ObjectProperty>

</rdf:RDF>