<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:fibo-fnd-arrx-agt="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/"
    xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-md-dbt-py="https://spec.edmcouncil.org/fibo/MD/DebtTemporal/DebtPricingYields/"
    xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
    xmlns:fibo-fnd-plc-loc="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"
    xmlns:fibo-ext-snap-par-wt="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Events</rdfs:label>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/MD/DebtTemporal/DebtPricingYields/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Something which occurs at a given time and place.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >event</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Locations/Location"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/where"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/Instant"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/when"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/EnablingAgent"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/agency"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/followedBy"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:scopeNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Re-framed as Instaneous event (per model which has Instant already); co-extensive with REA Business Event's nature. BUT REA Business Event has a transaction-specific definition which should be reflected by transaction-specific party relations (the association to Person in ISO 15944-4) Earlier Note: Note that the thing which occurs may be defined as a part of the model that refers to this "Event" for its time and place. Therefore Event does not show any specific properties for those other aspects of "what happens".</skos:scopeNote>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/EnablingAgent">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >enabling agent</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/where">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Locations/Location"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >where</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/followedBy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >followed by</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/causes">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/EnablingAgent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >causes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/agency">
    <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/causes"/>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/EnablingAgent"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >agency</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/when">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/Instant"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >when</rdfs:label>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/EnablingAgent">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/EnablingAgent"/>
  </rdf:Description>
</rdf:RDF>

