<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-ext-snap-par-wt "https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/">
	<!ENTITY fibo-fnd-arrx-agt "https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/">
	<!ENTITY fibo-fnd-occ-ev "https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
	<!ENTITY fibo-fnd-plc-loc "https://spec.edmcouncil.org/fibo/FND/Places/Locations/">
	<!ENTITY fibo-fnd-tim-tim "https://spec.edmcouncil.org/fibo/FND/Time/Time/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-md-dbt-py "https://spec.edmcouncil.org/fibo/MD/DebtTemporal/DebtPricingYields/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-ext-snap-par-wt="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"
	xmlns:fibo-fnd-arrx-agt="https://spec.edmcouncil.org/fibo/FND/Arrangements/EnablingAgents/"
	xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-fnd-plc-loc="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"
	xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-md-dbt-py="https://spec.edmcouncil.org/fibo/MD/DebtTemporal/DebtPricingYields/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Events</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/MD/DebtTemporal/DebtPricingYields/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-ev;EnablingAgent">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">enabling agent</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-ev;Event">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">event</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Something which occurs at a given time and place.</skos:definition>
		<skos:scopeNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Re-framed as Instaneous event (per model which has Instant already); co-extensive with REA Business Event&apos;s nature. BUT REA Business Event has a transaction-specific definition which should be reflected by transaction-specific party relations (the association to Person in ISO 15944-4) Earlier Note: Note that the thing which occurs may be defined as a part of the model that refers to this &quot;Event&quot; for its time and place. Therefore Event does not show any specific properties for those other aspects of &quot;what happens&quot;.</skos:scopeNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-ev;agency">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">agency</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-ev;causes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">causes</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-ev;followedBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">followed by</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-ev;when">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">when</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-ext-snap-par-wt;Instant"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-ev;where">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">where</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-plc-loc;Location"/>
	</owl:ObjectProperty>

</rdf:RDF>