<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-arrx-set "https://spec.edmcouncil.org/fibo/FND/Arrangements/SetMembership/">
	<!ENTITY fibo-fnd-bus-agc "https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/">
	<!ENTITY fibo-fnd-occ-act "https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/">
	<!ENTITY fibo-fnd-occ-ev "https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
	<!ENTITY fibo-fnd-occ-lif "https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/">
	<!ENTITY fibo-fnd-occ-occ "https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/">
	<!ENTITY fibo-fnd-occ-prod "https://spec.edmcouncil.org/fibo/FND/Occurrent/Production/">
	<!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Occurrent/Production/"
	xmlns="https://spec.edmcouncil.org/fibo/FND/Occurrent/Production/"
	xmlns:fibo-fnd-arrx-set="https://spec.edmcouncil.org/fibo/FND/Arrangements/SetMembership/"
	xmlns:fibo-fnd-bus-agc="https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/"
	xmlns:fibo-fnd-occ-act="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-fnd-occ-lif="https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/"
	xmlns:fibo-fnd-occ-occ="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/"
	xmlns:fibo-fnd-occ-prod="https://spec.edmcouncil.org/fibo/FND/Occurrent/Production/"
	xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Occurrent/Production/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">Production</rdfs:label>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesIssuance/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/SetMembership/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Lifecycles/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="http://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesIssuance/Offering">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-occ-prod;Offering"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-occ-act;Actor">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-occ-prod;Actor"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-occ-occ;Plan">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-occ-prod;Plan"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;Actor">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;productionRole"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-prod;Production"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">actor</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;EventProgram">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-prod;Program"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;hasProgrammedElement"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-ev;Event"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">event program</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A planned sequence of events.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;Offering">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-prod;ProductionOutcome"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">offering</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;Plan">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">plan</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;Production">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;PrescriptiveOccurrent"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;MediatingThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;context"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;follows"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-prod;Plan"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;produces"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-uo-lat;IndependentThing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">production</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A combination of an event and a context, in which something or some range of things are produced, created or issued.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;ProductionMilestone">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-lif;Milestone"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">production milestone</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;ProductionOutcome">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-occ;IntendedOccurrent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">production outcome</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;ProductionProgram">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-prod;Program"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;hasConstituent"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-prod;Production"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">production program</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A planned set of Productions.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-occ-prod;Program">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-set;Set"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-occ-prod;hasConstituent"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-uo-lat;OccurrentThing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">program</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A set of activities and / or events set up at one time by some agency with the intent to have those activities and / or events take place at some point in the future.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Modeled as a set of Productions. It may be necessary in future to distinguish programs which are a set of productions (this class) from programs which are a set of other types of activity.</skos:editorialNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;completes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">completes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;ProductionMilestone"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;context">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">context</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;follows">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">follows</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-prod;Plan"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-occ-prod;isPlanFor"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;givesRiseTo">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-occ-occ;culminatesIn"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">gives rise to</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-prod;ProductionOutcome"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;hasConstituent">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has constituent</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Program"/>
		<rdfs:range
			rdf:resource="&fibo-uo-lat;OccurrentThing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;hasProgrammedElement">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-occ-prod;hasConstituent"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has programmed element</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;EventProgram"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-ev;Event"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;isPlanFor">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">isPlanFor</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Plan"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;produces">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">produces</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
		<rdfs:range
			rdf:resource="&fibo-uo-lat;IndependentThing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-occ-prod;productionRole">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">production role</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-occ-prod;Actor"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-prod;Production"/>
	</owl:ObjectProperty>

</rdf:RDF>