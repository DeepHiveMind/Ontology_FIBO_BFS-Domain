<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-inf-idx="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"
    xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Identifiers</rdfs:label>
    <owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/IndependentThing">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >independent thing</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/IdentificationScheme">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A formal scheme defining identification codes for some purpose.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identification scheme</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/Identifier"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/defines.1"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/Scheme"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/Identifier">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A textual code which uniquely identifies something.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identifier</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/IdentificationScheme"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/definedIn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/uniquelyIdentifies"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/SchematicCode"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/hasIdentifier">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/Identifier"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has identifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/isIdentifiedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/hasIdentifierCode">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The textual value of the unique identification code. Editorial Note: Renamed to match the property in FIBO Foundations Relations ontology.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TextValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/Identifier"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has identifier code</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/code"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/uniquelyIdentifies">
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/Identifier"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >uniquely identifies</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/identifies"/>
  </owl:ObjectProperty>
</rdf:RDF>

