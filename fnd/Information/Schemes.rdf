      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/SchematicCode"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/CurrencyCodeScheme">
