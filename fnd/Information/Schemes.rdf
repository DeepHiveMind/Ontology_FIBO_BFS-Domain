  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/CountryCodeScheme">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >country code scheme</rdfs:label>
