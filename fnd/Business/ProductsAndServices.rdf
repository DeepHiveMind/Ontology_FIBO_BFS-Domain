    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/Product"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
