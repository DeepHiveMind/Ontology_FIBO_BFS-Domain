<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl1="http://www.w3.org/2002/07/owl/"
    xmlns:fibo-fnd-math-amt="https://spec.edmcouncil.org/fibo/FND/Math/Amounts/"
    xmlns:fibo-fnd-txn-ecr="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
    xmlns:fibo-fnd-txn-qr="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Amounts/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >QuantifiedResources</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/QuantifiedResource">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >An example would be Capital, which is an amount of money with a purpose.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >An amount of something which is defined in terms of its use for some purpose.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >quantified resource</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Amounts/NumericAmount"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/takesMaterialForm"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/EconomicResource"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/QuantifiedRawMaterial">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Some measured or quantified amount of some raw material. Scope Note: This is a kind of economic resource i.e. it may be bought or sold or exchanged, or put to work in some process.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >quantified raw material</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Amounts/MaterialAmount"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/quantifiedRawMaterialTakesMaterialForm"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/QuantifiedResource"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/quantifiedRawMaterialTakesMaterialForm">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The actual numerical amount of some material, that is represented as quantified raw material.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Amounts/MaterialAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/QuantifiedResources/QuantifiedRawMaterial"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >quantified raw material takes material form</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

