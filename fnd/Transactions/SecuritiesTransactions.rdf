<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-sec-sec-neg="https://spec.edmcouncil.org/fibo/SEC/Securities/NegotiableSecurities/"
    xmlns:fibo-fnd-txn-ecr="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"
    xmlns:fibo-fnd-txn-sec="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/"
    xmlns:fibo-fnd-txn-mkt="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/"
    xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-fnd-occ-proc="https://spec.edmcouncil.org/fibo/FND/Occurrent/Processes/"
    xmlns:fibo-fnd-txn-sup="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/NegotiableSecurities/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >SecuritiesTransactions</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/WhenIssuedTransaction">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Trading in securities ahead of them being traded.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >when issued transaction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionCounterparty">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >securities transaction counterparty</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/TransactionCounterparty"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SettlementProcess">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >settlement process</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionPrincipal">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >securities transaction principal</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/TransactionPrincipal"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionContract">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This is in line with the REA Ontology in which all Transactions are embodied in some Contract, whether written or implied. forms part of future "Transaction" model to be reviewed, but is ancestral to Options contract and transactions model.</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The contract (written or implied) which governs the transaction of securities in the secondary Market.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >securities transaction contract</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/governs"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/EconomicContract"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialPrimaryMarketTransaction">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >financial primary market transaction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SettlementProcess"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/follows"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/ContractualTransaction"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/MarketTransaction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction">
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/MarketTransaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionContract"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/embodies"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionCounterparty"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/counterparty"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/ContractualTransaction"/>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A Transaction in which some negotiable security is provided in exchange for some Consideration.</skos:definition>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/NegotiableSecurities/NegotiableSecurity"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/subject"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >financial securities secondary market transaction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/NegotiableCash"/>
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/NegotiableSecurities/NegotiableSecurity"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/consideration"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionPrincipal"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/MarketTransactions/principal"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/follows">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SettlementProcess"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialPrimaryMarketTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >follows</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/transactionEventFollowsBusinessProcess"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/offsetForWIIssueDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/WhenIssuedTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >offset for w i issue date</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Used to calculate actual settlement date from given WI issue date. If issue date is unknwn, this determines how many days form the issue date it's going to settle.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/governs">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/embodies"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >governs</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/economicAgreementGovernsTransaction"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/embodies">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/SecuritiesTransactionContract"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >embodies</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/transactionEmbodiesEconomicAgreement"/>
  </owl:ObjectProperty>
</rdf:RDF>

