bubu<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">conditional right</rdfs:label>
bubu<owl:disjointWith rdf:resource="&fibo-fnd-soc-asp;UnconditionalRight"/>
bu</owl:Class>
