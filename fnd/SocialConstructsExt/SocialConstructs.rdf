buxmlns:fibo-fnd-soc-soc="https://spec.edmcouncil.org/fibo/FND/SocialConstructsExt/SocialConstructs/"
buxmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
