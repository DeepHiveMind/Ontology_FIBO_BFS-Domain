		<owl:equivalentClass rdf:resource="&lcc-cr;Country"/>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fnd-plc-cty;GeopoliticalEntity">
		<owl:equivalentClass rdf:resource="&lcc-cr;GeopoliticalEntity"/>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fnd-plc-loc;Location">
		<owl:equivalentClass rdf:resource="&lcc-cr;Location"/>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-rel-rel;classifies">
		<owl:equivalentProperty rdf:resource="&lcc-cr;classifies"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-rel-rel;hasPart">
		<owl:equivalentProperty rdf:resource="&lcc-cr;hasPart"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-rel-rel;isClassifiedBy">
		<owl:equivalentProperty rdf:resource="&lcc-cr;isClassifiedBy"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-rel-rel;isPartOf">
		<owl:equivalentProperty rdf:resource="&lcc-cr;isPartOf"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-rel-rel;isUsedBy">
		<owl:equivalentProperty rdf:resource="&lcc-cr;isUsedBy"/>
