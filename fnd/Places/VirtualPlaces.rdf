    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/NetworkLocation -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/NetworkLocation">
        <rdfs:label>network location</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/VirtualLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/isIdentifiedBy"/>
                <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/Address"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A network address is location in a telecommunications network that may be identified by a network address (an identifier for a node or interface)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/NotionalPlace -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/NotionalPlace">
