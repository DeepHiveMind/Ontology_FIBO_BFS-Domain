        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A place described in terms of some abstract description or as a list of commonly understood concepts such as domestic, Eurozone etc.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/VirtualLocation -->

    <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/VirtualLocation">
        <rdfs:label>virtual location</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Locations/Location"/>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A place which has no physical location.</skos:definition>
    </owl:Class>
    


