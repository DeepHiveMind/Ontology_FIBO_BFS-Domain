    <!-- https://spec.edmcouncil.org/fibo/FND/UtilitiesExt/Values/hasValue -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/FND/UtilitiesExt/Values/hasValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FND/UtilitiesExt/Values/SingleValue"/>
        <rdfs:label xml:lang="en">hasValue</rdfs:label>
        <skos:definition xml:lang="en">hasValue is a datatype property of a SingleValue that represents the primary primitive data of that SingleValue. The range of hasValue is unrestricted as it is intended to be restricted in subtypes of SingleValue for a particular purpose with a particular data type.</skos:definition>
    </owl:DatatypeProperty>
    


