        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapPaymentCommitment"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/ProtectionBuyer">
    <skos:definition xml:lang="en">The party which buys protection against a default on the Reference Obligation, from the Protection Seller.</skos:definition>
    <owl:disjointWith>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/ProtectionSeller"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">protection buyer</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/DeliverableObligationSeller"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/PartiesExt/PartyExtensions/becomesParty"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CDSParty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CDSContingentSettlementCommitment">
    <rdfs:label xml:lang="en">c d s contingent settlement commitment</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/SettlementCommitment"/>
    <rdfs:subClassOf>
