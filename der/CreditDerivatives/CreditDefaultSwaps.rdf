        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CreditProtectionCommitment"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasContractualElement"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/ContingentTransactionCommitment"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/establishes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/hasSeller"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/ProtectionSeller"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:editorialNote xml:lang="en">Earlier draft definition: A swap instrument in which one party underwrites the risk of credit default of the other party or of an identified third party.</skos:editorialNote>
