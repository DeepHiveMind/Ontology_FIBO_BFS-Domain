          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/hasBuyer"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/DeliverableObligationSeller"/>
