  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CreditProtectionCommitment">
    <fibo-fnd-utl-av:explanatoryNote xml:lang="en">Terms are defined as "Contingent Leg"; in FpML these are defined as "Protection Terms". They are terms of the CDS contract.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition xml:lang="en">The commitment in a Credit Defualt Swap, to protect against specified credit events.</skos:definition>
    <rdfs:label xml:lang="en">credit protection commitment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CreditProtectionContingentLegTerms"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/definedAs.2"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapLegTerms"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/CreditDerivatives/CreditDefaultSwaps/CDSCashSettlementDealer">
    <skos:editorialNote xml:lang="en">ACTION not sure if this is a distinct party or specification of an existing party in a wider process. Assume the latter - so define where and what.</skos:editorialNote>
    <skos:definition xml:lang="en">A party from whom quotations are obtained by the calculation agent on the reference obligation for purposes of cash settlement of a CDS.</skos:definition>
    <rdfs:label xml:lang="en">c d s cash settlement dealer</rdfs:label>
  </owl:Class>
