<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-der-der-opt="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-fnd-txn-sec="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-der-ass-ass="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/"
    xmlns:fibo-der-ass-bo="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/"
    xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >BondOptions</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondUnderlying">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >bond underlying</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/DebtObservable"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/OTCBondOption">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >FpML Definition (unusable): A component describing a Bond Option product.</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Definition needed.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >o t c bond option</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondUnderlying"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/oTCBondOptionHasUnderlying"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondOptionUnderlyingTransaction"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/definesOptionalBondOptionUnderlyingTransaction"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/OTCOptionContract"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/DebtDerivativeContract"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondOptionUnderlyingTransaction">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A transaction which is defined in a bond option contract and which may or may not take place, at the discretion of the holder of the option. If it does take place, this will be a transaction for the purchase or sale of the deliverable underlying (in this case a bond) as defined in the option contract.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >bond option underlying transaction</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/FinancialSecuritiesSecondaryMarketTransaction"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/OptionalUnderlyingTransaction"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/underlyingBondPrice">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondOptionUnderlyingTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >underlying bond price</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The price at which the bond is bought and sold in the underlying transaction.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/oTCBondOptionHasUnderlying">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondUnderlying"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/OTCBondOption"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >o t c bond option has underlying</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/definesOptionalBondOptionUnderlyingTransaction">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/BondOptionUnderlyingTransaction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/BondOptions/OTCBondOption"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >defines optional bond option underlying transaction</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/definesOptional"/>
  </owl:ObjectProperty>
</rdf:RDF>

