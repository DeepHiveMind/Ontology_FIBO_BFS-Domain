    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/Warrant"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has warrant underlying</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/hasUnderlier"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/numberOfSecurities">
