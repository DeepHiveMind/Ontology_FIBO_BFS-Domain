  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/purchasePrice">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The price at which the holder of the Purchase Right may purchase the underlying security.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/Price"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/PurchaseRight"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >purchase price</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/givesTheRightToBuy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/StockholdersRightsInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
