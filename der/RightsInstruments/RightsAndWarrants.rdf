    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/Warrant"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has exercise terms</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasContractualElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/hasListing">
