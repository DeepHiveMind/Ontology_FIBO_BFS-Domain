    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ExerciseConventions/ExerciseDatesConvention"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/WarrantExerciseTermsSet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has exercise convention</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/isQualifiedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/givesRightToBuy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Equities/EquityInstruments/Share"/>
