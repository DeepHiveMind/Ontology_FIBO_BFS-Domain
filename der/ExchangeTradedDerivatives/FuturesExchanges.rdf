bubu<rdfs:subClassOf>
bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-der-etd-fxc;marginCall"/>
bubububu<owl:someValuesFrom rdf:resource="&fibo-fnd-utlx-val;TrueFalseValue"/>
bububu</owl:Restriction>
bubu</rdfs:subClassOf>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
