          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/hasOptionGuarantor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >exchange traded option contract</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/hasFeature"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/OptionBarrierFeatureTerms"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/OptionContract"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/TradedOptionsCounterparty"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/hasCounterparty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/DerivativesStandardizedTerms/DerivativesPriceDeterminationMethod"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/hasPriceDeterminationMethod"/>
        </owl:onProperty>
