        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/hasPrincipal"/>
