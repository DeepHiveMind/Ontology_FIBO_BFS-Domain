    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A share which is the underlying of an Equities Future contract.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >equity future share underlying</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Equities/EquityInstruments/PubliclyTradedShare"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/UnderlyingOnFuture"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/InterestRateFutureDebtUnderlying">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The debt instrument which is the underlying of an Interest Rate Future contract.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >interest rate future debt underlying</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/TradableDebtInstrument"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity.4"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/UnderlyingOnFuture"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/IndexFuture">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A futures contract on a stock or financial index. For each index there may be a different multiple for determining the price of the futures contract.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >index future</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/IndexDerivativeContract"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FinancialFuture"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/pricingDeterminedBy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/DerivativesStandardizedTerms/DerivativesPriceDeterminationMethod"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >pricing determined by</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/notionalAmount">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/InterestRateFuture"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/EquityFuture"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/CommodityFuture"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >notional amount</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The total value of a leveraged position's assets.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Equities/EquityInstruments/PubliclyTradedShare"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/EquityFutureShareUnderlying"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/contractSize">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >contract size</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The deliverable quantity of goods or commodities underlying the futures contract. This is denominated as a multiple of the lot size in the case of a commodity future. For financial futures this is the number of units of the underlying security.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/lotSize">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/CommodityFuture"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >lot size</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >How many of the underlying commodity can be sold. The groupings of certain units of measure in the underlying commodity.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity.4">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/TradableDebtInstrument"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/InterestRateFutureDebtUnderlying"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/hasListing">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/FuturesExchanges/FuturesListing"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has listing</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity.3">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/DerivativesStandardizedTerms/DerivativesExchange"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FuturesTradingAccountProvider"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/multiple">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/IndexFuture"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >multiple</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The multiple for determining the price of the futures contract in relation to the underlying index rate.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity.2">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/EconomicIndicator"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FuturesIndexUnderlying"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >index underlying identity</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/hasIdentity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/hasSettlementArrangement">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This determines what is delivered to the holder when the future is exercised.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The action by which an underlying commodity, security, cash value, or delivery instrument covering a contract is tendered and received by the contract holder.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/FuturesStandardizedTerms/DerivativesDeliveryType"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has settlement arrangement</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/identity.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/NegotiableCommodity"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FuturesCommodityUnderlying"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >identity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/securityDescription">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TextValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >security description</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The name of the security that uniquely identifies it amongst all other securities.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/hasProvider">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FuturesTradingAccountProvider"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has provider</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasPrincipal"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/lastDeliveryDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >last delivery date</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The last date in the range of dates by which the underlying for a futures contract must be delivered in order for the terms of the contract to be fulfilled.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/contractValue">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >contract value</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The deliverable value of goods, instruments or commodities underlying the futures contract.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/unitOfTrading">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FinancialFuture"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >unit of trading</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >How many of the underlying securities can be sold.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/lastNoticeDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >last notice date</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The last date at which a delivery notice can be issued.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/firstDeliveryDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >first delivery date</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The start date of the range of dates by which the underlying for a futures contract must be delivered in order for the terms of the contract to be fulfilled.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/tickSize">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >tick size</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The minimum value of the upward or downward movement of the price of the contract.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/conversionFactor">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/BondFuture"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >conversion factor</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The conversion factor is the price of the delivered bond/note ($1 par value) to yield a fixed rate. The conversion factor is used to calculate a final delivery price.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/tickValue">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >tick value</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The minimum value of the upward or downward movement of the contract. This is the contract size multiplied by the tick size.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/firstNoticeDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >first notice date</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The first date at which a delivery notice can be issued.</skos:definition>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/maturity">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The date at which the security settlement must occur. It is the end of the life of the future contract.</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/FutureInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >maturity</rdfs:label>
  </owl:DatatypeProperty>
