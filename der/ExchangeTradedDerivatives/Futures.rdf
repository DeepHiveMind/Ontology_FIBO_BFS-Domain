    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/UnderlyingOnFuture"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/ExchangeTradedDerivatives/Futures/EquityFuture">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A Futures instrument which is based on an equity security or securities, specifically a publicly issued and traded share.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >equity future</rdfs:label>
    <rdfs:subClassOf>
