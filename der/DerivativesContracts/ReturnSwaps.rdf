<?xml version="1.0"?>
<rdf:RDF
    xmlns:fibo-der-der-rsw="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-der-ass-ass="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/"
    xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
    xmlns:fibo-der-der-swx="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/SwapsExt/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/SwapsExt/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >ReturnSwaps</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnLeg">
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/PaymentLeg"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >return leg</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnCommitment"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/embodiesAssetReturnCommitment"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapLeg"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/SimpleReturnStream">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Does not include the appreciation on the price of the asset.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A leg defining the returns on an underlying instrument, namely the interest payments on a debt instrument or the dividends on an equity.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >simple return stream</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnCommitment">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >asset return commitment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/definedAs"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapLegTerms"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This does not refer to the return to the holder, but to the return on an underlying asset. This may be just the dividend / interest amounts on the underlying, or it may also include the increase / decrease in value of the underlying (Total Return). 10 Mar: Not used for Interest Rate Swaps. Return Leg is the non cash leg of a swap that has a cash leg. This leg type has nothing to do with Statistical leg types. A return for one negotiator is payment for the other. FpML: Return amounts of the return type swap.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A leg in which the returns on an asset are paid.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >asset return stream</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/AssetUnderlier"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/hasReferenceAsset"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/Swapstream"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnSwapTransaction">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >17 March review session notes: This is about the return you get not about what kinds of legs it has. Talk about return as a payoff. Then kinds of return are kinds of payoff e.g. total return. Return of a portfiolo, a single asset, or an index as per other note.</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The transaction of a total Return Swap. This is the transaction in which the return on some asset is exchanged for some agreed payment or series of payments.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >return swap transaction</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapTransaction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/TotalReturnSwapContract">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >total return swap contract</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnSwapContract"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnSwapContract">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >return swap contract</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/Swap"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetDerivativeContract"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetDerivativeContract">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >asset derivative contract</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/AssetUnderlier"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/hasUnderlier"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DerivativeInstrument"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/embodiesAssetReturnCommitment">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnCommitment"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnLeg"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >embodies asset return commitment</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/hasReturnLeg">
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/hasTransactionLeg"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnSwapTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has return leg</rdfs:label>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnLeg"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/hasReferenceAsset">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetDerivatives/AssetUnderlier"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has reference asset</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/definedAs">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnCommitment"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >defined as</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/hasFundingLeg">
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/hasTransactionLeg"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/ReturnSwapTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has funding leg</rdfs:label>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/PaymentLeg"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/rateOfReturn">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The terms of the initial price of the return type swap and of the subsequent valuations of the underlyer.</skos:definition>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/ReturnSwaps/AssetReturnStream"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >rate of return</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

