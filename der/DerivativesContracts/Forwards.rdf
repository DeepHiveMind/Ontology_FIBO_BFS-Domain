        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Forwards/ForwardContract"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Forwards/closedOutBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Forwards/ForwardDeliveryTermsSet"/>
  </owl:Class>
