      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Forwards/ForwardContractSeller"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Forwards/hasSeller"/>
        </owl:onProperty>
      </owl:Restriction>
