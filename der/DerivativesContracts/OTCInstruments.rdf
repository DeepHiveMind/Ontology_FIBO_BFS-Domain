<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY AccountsMain "https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/">
	<!ENTITY AnnotationVocabulary "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY Contracts "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY CurrencyAmount "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY DebtInstruments "https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/">
	<!ENTITY DerivativesBasics "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/">
	<!ENTITY FinancialInstruments "https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstruments/">
	<!ENTITY OTCDerivativeMasterAgreements "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/">
	<!ENTITY OTCDerivatives "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/">
	<!ENTITY OTCInstruments "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/">
	<!ENTITY REATransactions "https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/">
	<!ENTITY Securities "https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/"
	xmlns:AccountsMain="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"
	xmlns:AnnotationVocabulary="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:Contracts="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:CurrencyAmount="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:DebtInstruments="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/"
	xmlns:DerivativesBasics="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"
	xmlns:FinancialInstruments="https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstruments/"
	xmlns:OTCDerivativeMasterAgreements="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"
	xmlns:OTCDerivatives="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/"
	xmlns:OTCInstruments="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/"
	xmlns:REATransactions="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
	xmlns:Securities="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">OTCInstruments</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/"/>
	</owl:Ontology>
	
	<rdfs:Datatype
		rdf:about="&xsd;Date">
	</rdfs:Datatype>
	
	<rdfs:Datatype
		rdf:about="&xsd;String">
	</rdfs:Datatype>
	
	<owl:Class
		rdf:about="&OTCInstruments;CashDeliveryCommitment">
		<rdfs:subClassOf
			rdf:resource="&DerivativesBasics;DeliveryCommitment"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&OTCInstruments;commitsTo"/>
				<owl:someValuesFrom
					rdf:resource="&OTCInstruments;DeliverCash"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">cash delivery commitment</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Commitment to deliver cash at the earliest available date as per settlement convention. This is the same date as a settlement date when the corresponding cash is made available in the other currency, in settlement for this amount.</skos:definition>
		<AnnotationVocabulary:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">FpML exchangedCurrency1 referred to as a &quot;Currency Stream&quot;</AnnotationVocabulary:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;DeliverCash">
		<rdfs:subClassOf
			rdf:resource="&DerivativesBasics;DerivativeDeliveryTermsSet"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">deliver cash</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;OTCContractSettlementTerms">
		<rdfs:subClassOf
			rdf:resource="&Contracts;ContractTermsCompositeDescription"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c contract settlement terms</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;OTCDerivativeAgreement">
		<rdfs:subClassOf
			rdf:resource="&DerivativesBasics;DerivativeAgreement"/>
		<rdfs:subClassOf
			rdf:resource="&OTCInstruments;OTCTransactionAgreement"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c derivative agreement</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;OTCInstrumentConfirmation">
		<rdfs:subClassOf
			rdf:resource="&Contracts;WrittenContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&OTCDerivativeMasterAgreements;governs"/>
				<owl:someValuesFrom
					rdf:resource="&OTCDerivatives;OTCDerivativeTransaction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&Contracts;supersedes"/>
				<owl:someValuesFrom
					rdf:resource="&OTCInstruments;OTCInstrumentContract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c instrument confirmation</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A formal confirmation document which constitutes a written contract between the parties to the transaction, setting out the terms and conditions of the transaction.</skos:definition>
		<AnnotationVocabulary:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This effectively supersedes the (implied) contract that is brought into being by the transaction. It makes reference to the terms and conditions under which the transaction is deemed to have been conducted. The Confirmation references the Master Agreement under which the transaction has been conducted along with the ISDA standard terms referred to (if applicable), and adds terms and conditions specific to that one Transaction. This Confirmation generally supersedes all terms in the ISDA standard Definitions and the Master Agreement in the event of any inconsistency between the three.</AnnotationVocabulary:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;OTCInstrumentContract">
		<rdfs:subClassOf
			rdf:resource="&REATransactions;EconomicContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&OTCInstruments;oTCInstrumentContractEmbodies"/>
				<owl:someValuesFrom
					rdf:resource="&OTCInstruments;OTCTransactionAgreement"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c instrument contract</rdfs:label>
		<owl:disjointWith
			rdf:resource="&Securities;Security"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&OTCInstruments;OTCTransactionAgreement">
		<rdfs:subClassOf
			rdf:resource="&REATransactions;ContractualEconomicAgreement"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c transaction agreement</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The agreement which forms the basis of any Over the Counter (OTC) transaction, including OTC derivatives and bilateral debt instruments.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&OTCInstruments;commitsTo">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">commits to</rdfs:label>
		<rdfs:domain
			rdf:resource="&OTCInstruments;CashDeliveryCommitment"/>
		<rdfs:range
			rdf:resource="&OTCInstruments;DeliverCash"/>
	</owl:ObjectProperty>
	
	<rdf:Description
		rdf:about="&OTCInstruments;deliveryAmount">
		<rdf:type
			rdf:resource="&owl;DatatypeProperty"/>
		<rdf:type
			rdf:resource="&owl;ObjectProperty"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">delivery amount</rdfs:label>
		<rdfs:domain
			rdf:resource="&OTCInstruments;DeliverCash"/>
		<rdfs:range
			rdf:resource="&CurrencyAmount;MonetaryAmount"/>
	</rdf:Description>
	
	<owl:DatatypeProperty
		rdf:about="&OTCInstruments;deliveryDate">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">delivery date</rdfs:label>
		<rdfs:domain
			rdf:resource="&OTCInstruments;DeliverCash"/>
		<rdfs:range
			rdf:resource="&xsd;Date"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">&apos;The date on which both currencies traded will settle.&apos;</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&OTCInstruments;oTCInstrumentContractEmbodies">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c instrument contract embodies</rdfs:label>
		<rdfs:domain
			rdf:resource="&OTCInstruments;OTCInstrumentContract"/>
		<rdfs:range
			rdf:resource="&OTCInstruments;OTCTransactionAgreement"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The economic agreement (transaction related agreement) which the OTC Instrument Contract describes as obtaining between the parties to the Contract.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&OTCInstruments;paymentType">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">payment type</rdfs:label>
		<rdfs:domain
			rdf:resource="&OTCInstruments;DeliverCash"/>
		<rdfs:range
			rdf:resource="&xsd;String"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">FpML = paymentType &apos;A classification of the type of fee or additional payment, e.g. brokerage, upfront fee etc. FpML does not define domain values for this element.&apos;</skos:definition>
	</owl:DatatypeProperty>

</rdf:RDF>