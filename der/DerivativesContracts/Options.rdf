bu<owl:Class rdf:about="&fibo-der-der-opt;LookbackStrikeTerms">
bubu<rdfs:subClassOf rdf:resource="&fibo-der-der-opt;OptionStrikeTerms"/>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
