<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-txn-ecr="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"
    xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
    xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:fibo-der-der-sp="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-der-fx-sp="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-fnd-txn-sup="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/"
    xmlns:fibo-der-fx-ctr="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxContracts/"
    xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >FxSpots</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxContracts/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpot">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fx spot</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotTransaction"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/hasCorrespondingTransaction"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxContracts/OTCFxProduct"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotTransaction">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fx spot transaction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/NegotiableCash"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/subject"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotCashSettlementTerms"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/hasSide"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/DeliverCash"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/hasSide"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/hasImpliedContract"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotTransaction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Though the spot exchange rate is said to be settled immediately, the globally accepted settlement cycle for foreign-exchange contracts is two days. Foreign-exchange contracts are therefore settled on the second day after the day the deal is made.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The rate of a foreign-exchange contract for immediate delivery. Also known as "benchmark rates", "straightforward rates" or "outright rates", spot rates represent the price that a buyer expects to pay for a foreign currency in another currency.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fx spot contract</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotSettlementCommitment"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/establishes.1"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/CashDeliveryCommitment"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/establishes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/CurrencySpotBuyRate"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/hasUnderlier"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotContract"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/OTCInstrument"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotSettlementCommitment">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >FpML exchangedCurrency2 referred to as a "Currency Stream"</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >no definition</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fx spot settlement commitment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotCashSettlementTerms"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/commitsTo"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotSettlementCommitment"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/fxSpotRate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >fx spot rate</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >'The rate of exchange between the two currencies.'</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/hasCorrespondingTransaction">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotTransaction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpot"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has corresponding transaction</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/establishes.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotSettlementCommitment"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >establishes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/sellerCurrency">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/Currency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >seller currency</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The seller's currency as specified in the contract.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/buyerCurrency">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/Currency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >buyer currency</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The buyer's currency as specified in the contract.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/establishes">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/CashDeliveryCommitment"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >establishes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/commitsTo">
    <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/embodies"/>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Spots/SpotCashSettlementTerms"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotSettlementCommitment"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >commits to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/hasSide">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/SettlementTerms"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/FxDerivatives/FxSpots/FxSpotTransaction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has side</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

