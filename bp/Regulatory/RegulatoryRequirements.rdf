    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >RegulatoryRequirements</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/OperationalRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >operational requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRequirement"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryRegistrationRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >regulatory registration requirement</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRequirement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A requirement driven by business imperatives.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >business requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/Requirement"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryReportingRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >regulatory reporting requirement</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryOperationalRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
