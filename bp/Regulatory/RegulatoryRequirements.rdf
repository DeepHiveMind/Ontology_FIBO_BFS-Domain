    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/OperationalRule">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >operational rule</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRule"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/Requirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >requirement</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Something that must be done or refrained from</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRule">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A rule which applies in some business context. Includes operations rules, regulatory rules and so on.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >business rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/Rule"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/SystemRequirement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A requirement to be met in some System.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >system requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/ProductRequirement"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/LegalRequirement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A requirement driven by legal instruments, that is a requirement which is defined in and mandated by some body of statute law.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >legal requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/StatuteLaw"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mandatedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/Requirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/ProductRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >product requirement</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/StatuteLaw">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >statute law</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryRule">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A rule which has compliance mandates backed by some Government, with associated lenalties.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >regulatory rule</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/OperationalRule"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mayDrive"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/formalizes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRule"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RiskMitigationRequirement">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >risk mitigation requirement</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/BusinessRequirement"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/formalizes">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryRule"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >formalizes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mandatedBy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/StatuteLaw"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/LegalRequirement"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >mandated by</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mayDrive">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/OperationalRule"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/RegulatoryRule"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >may drive</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mandates">
    <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mandatedBy"/>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/LegalRequirement"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/StatuteLaw"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >mandates</rdfs:label>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/StatuteLaw">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/StatuteLaw"/>
  </rdf:Description>
</rdf:RDF>

