xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:lcc-lr="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"
     xmlns:lcc-cr="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/">


    <owl:Ontology rdf:about="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/">
        <rdfs:label>Country and Subdivision Representation Ontology</rdfs:label>


