        <rdf:type rdf:resource="&lcc-lr;IdentificationScheme"/>
        <rdfs:label>ISO 3166-2 code set</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the set of subdivision identifiers that comprise the ISO 3166-2 specification</skos:definition>
        <lcc-lr:comprises rdf:resource="&lcc-cr;SubdivisionCode"/>
        <lcc-lr:defines rdf:resource="&lcc-cr;SubdivisionCode"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&lcc-3166-2;District">
        <rdf:type rdf:resource="&lcc-cr;SubdivisionKind"/>
        <rdfs:label>district</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a country subdivision that is a type of administrative or territorial division established by a government for a specific purpose that, in some countries, is managed by a local government</skos:definition>
        <skos:note rdf:datatype="&xsd;anyURI">Across the world, areas known as districts vary greatly in size, spanning entire regions or counties, several municipalities, subdivisions of municipalities, school district, or political district.</skos:note>
        <dct:source rdf:datatype="&xsd;anyURI">http://www.merriam-webster.com/dictionary/district</dct:source>
        <dct:source rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/District</dct:source>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&lcc-3166-2;OutlyingArea">
        <rdf:type rdf:resource="&lcc-cr;SubdivisionKind"/>
        <rdfs:label>outlying area</rdfs:label>
        <skos:prefLabel>outlying area</skos:prefLabel>
        <skos:altLabel>insular area</skos:altLabel>
        <skos:definition rdf:datatype="&xsd;string">a country subdivision that is a type of administrative or territorial division that is typically remote from the main geopolitical entity</skos:definition>
        <dct:source rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Insular_area</dct:source>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&lcc-3166-2;State">
        <rdf:type rdf:resource="&lcc-cr;SubdivisionKind"/>
        <rdfs:label>state</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a country subdivision that is one of the constituent units of a federal sovereign state such as the United States, Australia, India and Brazil</skos:definition>
        <dct:source rdf:datatype="&xsd;anyURI">http://www.merriam-webster.com/dictionary/state/</dct:source>
        <dct:source rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/State</dct:source>
    </owl:NamedIndividual>

