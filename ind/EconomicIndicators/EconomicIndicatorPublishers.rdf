     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-rel-rel="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-org-fm="http://www.omg.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"
     xmlns:fibo-be-fct-pub="http://www.omg.org/spec/EDMC-FIBO/BE/FunctionalEntities/Publishers/"
     xmlns:fibo-be-ge-ge="http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/"
     xmlns:fibo-ind-ind-ind="http://www.omg.org/spec/EDMC-FIBO/IND/Indicators/Indicators/"
     xmlns:fibo-ind-ei-ei="http://www.omg.org/spec/EDMC-FIBO/IND/EconomicIndicators/EconomicIndicators/"
     xmlns:fibo-ind-ei-pub="http://www.omg.org/spec/EDMC-FIBO/IND/EconomicIndicators/EconomicIndicatorPublishers/">


    <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/IND/EconomicIndicators/EconomicIndicatorPublishers/">
        <rdfs:label>Economic Indicator Publishers Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO IND Economic Indicator Publishers Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2014-2016 EDM Council, Inc.
Copyright (c) 2014-2016 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO IND Economic Indicator Publishers Ontology -->

        <sm:filename rdf:datatype="&xsd;string">EconomicIndicatorPublishers.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-ind-ei-pub</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://www.omg.org/spec/EDMC-FIBO/IND/20160801/EconomicIndicators/EconomicIndicatorPublishers/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology provides concepts descriptive of the publishers of market indicators, such as gross domestic product, employment statistics, inflation rates and so on. These may include government or quasi-government bodies, international agencies, third parties and data providers.</sm:fileAbstract>

        <skos:changeNote rdf:datatype="&xsd;string">The http://www.omg.org/spec/EDMC-FIBO/IND/20150501/EconomicIndicators/EconomicIndicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 3 report.</skos:changeNote>
        <skos:changeNote rdf:datatype="&xsd;string">The http://www.omg.org/spec/EDMC-FIBO/IND/20140601/EconomicIndicators/EconomicIndicatorPublishers.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF report.</skos:changeNote>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/FND/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/BE/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/IND/Indicators/Indicators/</sm:dependsOn>
