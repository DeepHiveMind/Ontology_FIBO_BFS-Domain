    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Publications/Publications/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/IndicatorsValues/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/DER/AssetDerivatives/AssetBaskets/"/>
