  <owl:Ontology rdf:about="">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >BasketIndices</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/"/>
