         <triple xmi:type="odm:Triple">
            <RDFsubject xmi:type="odm:URIReferenceNode">
               <resource xmi:idref="number"/>
            </RDFsubject>
            <RDFpredicate href="localref://././.#www.w3.org/2004/02/skos/core#editorialNote"/>
            <RDFobject xmi:type="odm:TypedLiteral">
               <datatype href="localref://././.#www.w3.org/2001/XMLSchema#string"/>
               <lexicalForm>Where there are specific business reasons or business knowledge defining numbers with known limits such as maximum values, positive-only and so on, specific types will be defined accordingly.</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
            <RDFsubject xmi:type="odm:URIReferenceNode">
               <resource xmi:idref="percentage"/>
            </RDFsubject>
            <RDFpredicate href="localref://././.#www.w3.org/2004/02/skos/core#definition"/>
            <RDFobject xmi:type="odm:TypedLiteral">
               <datatype href="localref://././.#www.w3.org/2001/XMLSchema#string"/>
               <lexicalForm>In mathematics, a percentage is a number or ratio as a fraction of 100. It is often denoted using the percent sign, %, or the abbreviation, pct.</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
