         <owlUniverse xmi:type="odm:OWLObjectProperty" isInverseFunctional="false"
                      isTransitive="false"
                      isSymmetric="false"
                      name="holdsControllingInterestIn"
                      xmi:uuid="www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/holdsControllingInterestIn"
                      xmi:id="holdsControllingInterestIn">
            <shortDescription>a company in which the voting shareholder owns some voting equity stake.</shortDescription>
            <RDFSsubPropertyOf href="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/OwnershipParties/holdsAnInterestIn"/>
            <RDFSdomain xmi:idref="VotingShareholder"/>
            <RDFSrange xmi:idref="www.omg.org_spec_EDMC-FIBO_BE_Corporations_Corporations_IncorporatedCompany"/>
         </owlUniverse>
