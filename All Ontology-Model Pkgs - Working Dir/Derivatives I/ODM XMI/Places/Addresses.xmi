            <RDFobject xmi:type="odm:TypedLiteral">
               <datatype href="localref://././.#www.w3.org/2001/XMLSchema#string"/>
               <lexicalForm>The physical area uniquely identified by some postal code.</lexicalForm>
            </RDFobject>
         </triple>
