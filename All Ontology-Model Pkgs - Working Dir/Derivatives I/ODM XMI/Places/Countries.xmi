            <RDFobject xmi:type="odm:TypedLiteral">
               <datatype href="localref://././.#www.w3.org/2001/XMLSchema#anyURI"/>
               <lexicalForm>http://www.thefreedictionary.com/urban+center</lexicalForm>
            </RDFobject>
         </triple>
