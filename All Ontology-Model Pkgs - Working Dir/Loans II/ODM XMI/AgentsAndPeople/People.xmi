            <RDFpredicate href="localref://././.#www.w3.org/2004/02/skos/core#altLabel"/>
            <RDFobject xmi:type="odm:PlainLiteral">
               <lexicalForm>national identity number</lexicalForm>
            </RDFobject>
         </triple>
         <triple xmi:type="odm:Triple">
            <RDFsubject xmi:type="odm:URIReferenceNode">
               <resource xmi:idref="NationalIdentificationNumber"/>
            </RDFsubject>
            <RDFpredicate href="localref://././.#www.w3.org/2004/02/skos/core#altLabel"/>
            <RDFobject xmi:type="odm:PlainLiteral">
               <lexicalForm>national insurance number</lexicalForm>
            </RDFobject>
