bubu<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/StateEntities/"/>
bubu<fibo-fnd-utl-av:hasMaturityLevel rdf:resource="&fibo-fnd-utl-av;Informative"/>
bu</owl:Ontology>
