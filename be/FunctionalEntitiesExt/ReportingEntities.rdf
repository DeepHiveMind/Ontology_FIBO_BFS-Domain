    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has statutory reporting obligation</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/hasObligation"/>
