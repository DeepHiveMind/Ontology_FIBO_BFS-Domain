    >is about</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/incorporatedUnder">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/StatuteLaw"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/StatutoryAuthority"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >incorporated under</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/hasCorporateFilingObligation">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/CorporateFilingObligation"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/BE/FunctionalEntitiesExt/ReportingEntities/StockCorporation"/>
