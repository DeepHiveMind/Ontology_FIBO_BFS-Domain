bubu<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntitiesExt/LEICapacity/"/>
bubu<fibo-fnd-utl-av:hasMaturityLevel rdf:resource="&fibo-fnd-utl-av;Informative"/>
