        <owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/20180501/AboutBE-2.0-EuropeAndNorthAmerica/"/>
		<dct:abstract rdf:datatype="&xsd;string">The 'about' BE 2.0 Europe and North American Extension ontology is provided for convenience for FIBO users.  It imports all of the Production (Released) ontologies that comprise the FIBO Business Entities (BE) Version 2.0 domain, including all of FND, and all of the European and North American individuals in FIBO production to date.</dct:abstract>

        <sm:publicationDate rdf:datatype="&xsd;dateTime">2018-05-21T18:00:00</sm:publicationDate>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/AboutBE-2.0-Europe/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/AboutBE-2.0-NorthAmerica/"/>

    </owl:Ontology>


