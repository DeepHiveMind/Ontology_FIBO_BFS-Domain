fibo-be-le-lei:SovereignState
        a                            owl:Class ;
        rdfs:label                   "sovereign state" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        rdfs:subClassOf
                [ a                   owl:Restriction ;
                  owl:onProperty      fibo-fnd-rel-rel:represents ;
                  owl:someValuesFrom  fibo-fnd-plc-cty:Country
                ] ;
        fibo-fnd-utl-av:adaptedFrom  "http://en.wikipedia.org/wiki/Sovereign_state"^^xsd:anyURI , "http://www.businessdictionary.com/definition/sovereign-authority.html"^^xsd:anyURI ;
        fibo-fnd-utl-av:explanatoryNote
                "It is also normally understood that a state is neither dependent on nor subject to any other power or state. States are defined as legal entities that are not organizations.  Typically only the government of a state can obligate or bind the state, for example by treaty."^^xsd:string ;
        owl:disjointWith             fibo-be-le-lei:SupranationalEntity ;
        skos:definition              "a non-physical, legal entity that is represented by one centralized government, has a permanent population, defined territory, and the capacity to enter into relations with other sovereign states"^^xsd:string .

fibo-be-le-lei:LegalEntityIdentifierScheme
        a                owl:Class ;
        rdfs:label       "legal entity identifier scheme" ;
        rdfs:subClassOf
                fibo-be-le-fbo:OrganizationIdentificationScheme ;
        rdfs:subClassOf
                [ a                  owl:Restriction ;
                  owl:allValuesFrom  fibo-be-le-lei:LegalEntityIdentifier ;
                  owl:onProperty     fibo-fnd-rel-rel:defines
                ] ;
        skos:definition  "the scheme that defines the LEI"^^xsd:string .

fibo-be-le-lei:MunicipalEntity
        a                 owl:Class ;
        rdfs:label        "municipal entity" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        owl:disjointWith  fibo-be-le-lei:SovereignState , fibo-be-le-lei:SupranationalEntity ;
        skos:definition   "Municipal entity in the sense of a legal entity, that is, what it is that incurs debt for a municipality, i.e. the Municipality in the sense of the legal entity, as distinct from the Municipal government. Draft definition: The legal entity which is a city or municipality. This has a Government which sets laws applicable within the geographical area corresponding to this legal entity. It is identified as a legal entity as well as the setter of laws because it is able to incur debt in its own right."^^xsd:string .

fibo-be-le-lp:LegalEntity
        a       owl:Class ;
        rdfs:subClassOf
                [ a                         owl:Restriction ;
                  owl:onClass               fibo-be-le-lei:LegalEntityIdentifier ;
                  owl:onProperty            fibo-fnd-aap-agt:isIdentifiedBy ;
                  owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                ] .

fibo-be-le-lei:SupranationalEntity
        a                            owl:Class ;
        rdfs:label                   "supranational entity" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        fibo-fnd-utl-av:adaptedFrom  "ISO 17442, Financial services - Legal Entity Identifier (LEI), section 3.2"^^xsd:string , "http://www.investopedia.com/terms/s/supranational.asp"^^xsd:anyURI ;
        fibo-fnd-utl-av:explanatoryNote
                "The European Union and the World Trade Organization are both supranationals. In the EU, each member votes on policy that will affect each member nation. The benefits of this construct for the EU are the synergies derived from social and economic policies along with a stronger presence on the international stage."^^xsd:string ;
        skos:definition              "a governmental or non-governmental entity that is established by international law or treaty or incorporated at an international level; an international organization, or union, whereby member states transcend national boundaries or interests to share in the decision-making and vote on issues pertaining to the wider grouping"^^xsd:string .

fibo-be-le-lei:RegionalGovernmentalEntity
        a                            owl:Class ;
        rdfs:label                   "regional governmental entity" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        rdfs:subClassOf
                [ a                   owl:Restriction ;
                  owl:onProperty      fibo-fnd-rel-rel:represents ;
                  owl:someValuesFrom  fibo-fnd-plc-cty:GeopoliticalEntity
                ] ;
        fibo-fnd-utl-av:adaptedFrom  "ISO 3166-2 Codes for the representation of names of countries and their subdivisions - Part 1, Country subdivision codes"^^xsd:string , "http://en.wikipedia.org/wiki/Administrative_division"^^xsd:anyURI ;
        owl:disjointWith             fibo-be-le-lei:SovereignState ;
        skos:definition              "a legal entity that corresponds to an administrative division, administrative unit, administrative entity or country subdivision (or, sometimes, geopolitical division or subnational entity), that has the capacity to incur debt, issue contracts, and enter into relations with other similar entities"^^xsd:string ;
        skos:example                 "For example, a country may be divided into provinces, which, in turn, are divided into counties, which, in turn, may be divided in whole or in part into municipalities; and so on."^^xsd:string .

fibo-be-le-lei:hasAddressOfLegalFormation
        a                   owl:ObjectProperty ;
        rdfs:domain         fibo-be-le-lp:LegalEntity ;
        rdfs:label          "has address of legal formation" ;
        rdfs:range          fibo-be-le-fbo:RegisteredAddress ;
        rdfs:subPropertyOf  fibo-be-le-fbo:hasRegisteredAddress ;
        skos:definition     "indicates the address of legal formation for the entity, in the jurisdiction in which the entity is established, used for registration purposes with respect to obtaining an LEI"^^xsd:string .

fibo-be-le-lei:  a           owl:Ontology ;
        rdfs:label           "Legal Entity Identifier (LEI) Entities Ontology" ;
        rdfs:seeAlso         "http://spec.edmcouncil.org/fibo/BE/LegalEntities/AboutLegalEntities/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/BE/AboutBE/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
        dct:license          "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
        sm:contentLanguage   "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI , "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
        sm:copyright         "Copyright (c) 2013-2015 EDM Council, Inc.\nCopyright (c) 2013-2015 Object Management Group, Inc."^^xsd:string ;
        sm:dependsOn         "http://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/FND/"^^xsd:anyURI ;
        sm:fileAbbreviation  "fibo-be-le-lei"^^xsd:string ;
        sm:fileAbstract      "This ontology defines concepts around contractually capable business entities. The terms defined here are those which are relevant to the Legal Entity Identifier (LEI) work. The term known as legal entity in that work is identified as a formal organization which is recognized in some jurisdiction as being capable of incurring some liability, whether or not is a legal person as understood by the legal community. This is labeled as contractually capable entity, to avoid confusion with the accepted legal term for Legal Entity. Such entities are recognized as requiring an LEI, but the identifier itself is allocated to the formal organization which is recognized as being contractually capable."^^xsd:string ;
        sm:filename          "LEIEntities.rdf"^^xsd:string ;
        owl:imports          fibo-fnd-law-jur: , fibo-fnd-rel-rel: , fibo-be-le-lp: , fibo-be-le-fbo: , fibo-fnd-aap-agt: , fibo-fnd-utl-av: , fibo-fnd-plc-cty: , fibo-fnd-pty-rl: , fibo-fnd-plc-adr: ;
        owl:versionIRI       <http://spec.edmcouncil.org/fibo/BE/20150801/LegalEntities/LEIEntities/> ;
        skos:changeNote      "The http://spec.edmcouncil.org/fibo/BE/20131101/LegalEntities/LEIEntities.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report."^^xsd:string .

fibo-be-le-lei:LegalEntityIdentifier
        a                owl:Class ;
        rdfs:label       "legal entity identifier" ;
        rdfs:subClassOf
                fibo-be-le-fbo:OrganizationIdentifier ;
        rdfs:subClassOf
                [ a                         owl:Restriction ;
                  owl:onClass               fibo-be-le-lp:LegalEntity ;
                  owl:onProperty            fibo-fnd-aap-agt:identifies ;
                  owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                ] ;
        skos:definition  "an organization identifier that uniquely identifies a legal entity as defined in ISO 17442"^^xsd:string .

fibo-be-le-lei:ContractuallyCapableEntity
        a                            owl:Class ;
        rdfs:label                   "contractually capable entity" ;
        rdfs:subClassOf
                fibo-fnd-pty-rl:AgentInRole ;
        rdfs:subClassOf
                [ a                         owl:Restriction ;
                  owl:onClass               fibo-be-le-lp:LegalEntity ;
                  owl:onProperty            fibo-fnd-rel-rel:hasIdentity ;
                  owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                ] ;
        fibo-fnd-utl-av:adaptedFrom  "ISO 17442"^^xsd:string ;
        skos:definition              "A unique entity that is legally or financially responsible for the performance of financial transactions, or has the legal right in its jurisdiction to enter independently into legal contracts, regardless of whether it is incorporated or constituted in some other way (e.g. trust, partnership, contractual). This excludes natural persons, but includes governmental organizations and supranationals."^^xsd:string .
