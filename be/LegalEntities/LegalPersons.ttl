fibo-be-le-lp:  a            owl:Ontology ;
        rdfs:label           "Legal Persons Ontology" ;
        rdfs:seeAlso         "http://spec.edmcouncil.org/fibo/BE/LegalEntities/AboutLegalEntities/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI , "http://spec.edmcouncil.org/fibo/BE/AboutBE/"^^xsd:anyURI ;
        dct:license          "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
        sm:contentLanguage   "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI , "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
        sm:copyright         "Copyright (c) 2013-2015 EDM Council, Inc.\nCopyright (c) 2013-2015 Object Management Group, Inc."^^xsd:string ;
        sm:dependsOn         "http://spec.edmcouncil.org/fibo/FND/"^^xsd:anyURI ;
        sm:fileAbbreviation  "fibo-be-le-lp"^^xsd:string ;
        sm:fileAbstract      "This ontology defines legal personhood concepts. A legal person as defined here is any natural person or organization which is capable of accruing liability on its own part."^^xsd:string ;
        sm:filename          "LegalPersons.rdf"^^xsd:string ;
        owl:imports          fibo-fnd-law-lcap: , fibo-fnd-org-fm: , fibo-fnd-aap-agt: , fibo-fnd-rel-rel: , fibo-fnd-utl-av: , fibo-fnd-pty-rl: , fibo-fnd-pty-pty: , fibo-fnd-law-jur: , fibo-fnd-aap-ppl: ;
        owl:versionIRI       <http://spec.edmcouncil.org/fibo/BE/20150801/LegalEntities/LegalPersons/> ;
        skos:changeNote      "The http://spec.edmcouncil.org/fibo/BE/20131101/LegalEntities/LegalPersons.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report."^^xsd:string .

fibo-be-le-lp:CharteredBody
        a                   owl:Class ;
        rdfs:label          "chartered body" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        skos:definition     "a legal entity created by a royal charter or decree"^^xsd:string ;
        skos:editorialNote  "In a monarchy or principality, the monarch typically vests the power to create such bodies, in an entity called (for example) the Privy Council."^^xsd:string ;
        skos:example        "Anything with 'Royal Institute' in the name. Also universities are generally set up by royal charter in a monarchy or principality, (often pre-dating any Privy Council i.e. directly be the monarch in the case of older universities). The Bank of England and the British Broadcasting Council (BBC) are also incorporated through Royal Charter."^^xsd:string .

fibo-be-le-lp:JuridicalPerson
        a                        owl:Class ;
        rdfs:label               "juridical person" ;
        rdfs:subClassOf
                fibo-fnd-org-fm:FormalOrganization ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        fibo-fnd-utl-av:definitionOrigin
                "Black's Law Dictionary Free Online, see http://thelawdictionary.org/juridical-person/"^^xsd:string ;
        fibo-fnd-utl-av:synonym  "artificial person"^^xsd:string , "juridical entity"^^xsd:string , "juristic person"^^xsd:string ;
        skos:definition          "an entity, as a firm, that is not a single natural person, as a human being, authorized by law with duties and rights, recognized as a legal authority having a distinct identity, a legal personality"^^xsd:string .

fibo-be-le-lp:designatesSignatory
        a                   owl:ObjectProperty ;
        rdfs:domain         fibo-be-le-lp:LegalPerson ;
        rdfs:label          "designates signatory" ;
        rdfs:range          fibo-be-le-lp:Signatory ;
        rdfs:subPropertyOf  fibo-fnd-rel-rel:designates ;
        skos:definition     "designates a party able to sign contracts on the part of the legal person"^^xsd:string .

fibo-be-le-lp:NaturalPerson
        a                              owl:Class ;
        rdfs:label                     "natural person" ;
        rdfs:subClassOf  fibo-fnd-aap-ppl:Adult ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalPerson ;
        owl:disjointWith               fibo-fnd-aap-ppl:IncapacitatedAdult ;
        skos:definition                "A person of maturity who in the eyes of the law is able to assume obligations."^^xsd:string .

fibo-be-le-lp:LegalPerson
        a                            owl:Class ;
        rdfs:label                   "legal person" ;
        rdfs:subClassOf
                fibo-fnd-aap-agt:AutonomousAgent ;
        rdfs:subClassOf
                [ a                            owl:Restriction ;
                  owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                  owl:onClass                  fibo-fnd-law-lcap:LiabilityCapacity ;
                  owl:onProperty               fibo-fnd-law-lcap:hasCapacity
                ] ;
        rdfs:subClassOf
                [ a                            owl:Restriction ;
                  owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                  owl:onClass                  fibo-fnd-law-jur:Jurisdiction ;
                  owl:onProperty               fibo-be-le-lp:isRecognizedIn
                ] ;
        fibo-fnd-utl-av:adaptedFrom  "http://en.wikipedia.org/wiki/Legal_personality"^^xsd:anyURI ;
        fibo-fnd-utl-av:explanatoryNote
                "To have legal personality means to be capable of having legal rights and duties within a certain legal system, such as to enter into contracts, sue, and be sued. Legal personality is a prerequisite to legal capacity, the ability of any legal person to amend (enter into, transfer, etc.) rights and obligations."^^xsd:string ;
        skos:definition              "any entity which can incur legal obligation and can be sued at law"^^xsd:string .

fibo-be-le-lp:isRecognizedIn
        a                owl:ObjectProperty ;
        rdfs:domain      fibo-be-le-lp:LegalPerson ;
        rdfs:label       "is recognized in" ;
        rdfs:range       fibo-fnd-law-jur:Jurisdiction ;
        skos:definition  "indicates the jurisdiction whose laws determine what is meant by legal, with respect to a legal person"^^xsd:string .

fibo-be-le-lp:isOrganizedIn
        a                   owl:ObjectProperty ;
        rdfs:domain         fibo-be-le-lp:LegalEntity ;
        rdfs:label          "is organized in" ;
        rdfs:range          fibo-fnd-law-jur:Jurisdiction ;
        rdfs:subPropertyOf  fibo-be-le-lp:isRecognizedIn ;
        skos:definition     "indicates the juridiction whose laws a legal entity is organized under"^^xsd:string .

fibo-be-le-lp:Signatory
        a                owl:Class ;
        rdfs:label       "signatory" ;
        rdfs:subClassOf
                fibo-fnd-pty-pty:PartyInRole ;
        rdfs:subClassOf
                [ a                         owl:Restriction ;
                  owl:onClass               fibo-be-le-lp:NaturalPerson ;
                  owl:onProperty            fibo-fnd-rel-rel:hasIdentity ;
                  owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                ] ;
        rdfs:subClassOf
                [ a                   owl:Restriction ;
                  owl:onProperty      fibo-fnd-pty-rl:isPlayedBy ;
                  owl:someValuesFrom  [ a                            owl:Restriction ;
                                        owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass                  fibo-fnd-law-lcap:SignatoryCapacity ;
                                        owl:onProperty               fibo-fnd-law-lcap:hasCapacity
                                      ]
                ] ;
        skos:definition  "Some agent who has the capacity to sign contracts on the part of some legal person"^^xsd:string .

fibo-be-le-lp:PowerOfAttorney
        a                owl:Class ;
        rdfs:label       "power of attorney" ;
        rdfs:subClassOf
                fibo-fnd-law-lcap:LegalCapacity ;
        rdfs:subClassOf
                [ a                            owl:Restriction ;
                  owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                  owl:onClass                  fibo-be-le-lp:NaturalPerson ;
                  owl:onProperty               fibo-fnd-rel-rel:isConferredOn
                ] ;
        fibo-fnd-utl-av:definitionOrigin
                "http://www.businessdictionary.com/definition/power-of-attorney.html"^^xsd:anyURI ;
        skos:definition  "Written legal authority given by one party (the principal) to another (the agent or attorney-in-fact) to act on the principals behalf. It may be a (1) General power Of attorney that authorizes the agent to act generally on behalf of the principal, or (2) Special power of attorney that is limited to a specific act or situation. Decisions made and actions taken by an attorney in fact (within the scope of his or her authority) are legally binding on the principal. A power of attorney can remain in force only so long as the principal is alive and does not become disabled or incapacitated. Although it is often conferred on the principals attorney, one does not need to be an attorney at law to be an attorney-in-fact."^^xsd:string .

fibo-be-le-lp:StatutoryBody
        a                            owl:Class ;
        rdfs:label                   "statutory body" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalEntity ;
        fibo-fnd-utl-av:adaptedFrom  "http://www.collinsdictionary.com/dictionary/english/statutory-body"^^xsd:anyURI ;
        skos:definition              "a body set up by a government to consider evidence and make judgements in some field of activity"^^xsd:string .

fibo-be-le-lp:LegalEntity
        a                            owl:Class ;
        rdfs:label                   "legal entity" ;
        rdfs:subClassOf
                fibo-be-le-lp:LegalPerson ;
        rdfs:subClassOf
                [ a                         owl:Restriction ;
                  owl:onClass               fibo-fnd-law-jur:Jurisdiction ;
                  owl:onProperty            fibo-be-le-lp:isOrganizedIn ;
                  owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                ] ;
        fibo-fnd-utl-av:adaptedFrom  "ISO 17442, Financial services - Legal Entity Identifier (LEI), first edition, 2012-06-01, section 3.1"^^xsd:string , "Barron's Dictionary of Banking Terms, Sixth Edition, 2012, definition of legal entity"^^xsd:string ;
        owl:disjointWith             fibo-be-le-lp:NaturalPerson ;
        skos:definition              "a partnership, corporation, or other organization having the capacity to negotiate contracts, assume financial obligations, and pay off debts, organized under the laws of some jurisdiction"^^xsd:string ;
        skos:example                 "Examples of eligible legal entities include, without limitation:\n-   all financial intermediaries;\n-   banks and finance companies;\n-   all entities that issue equity, debt or other securities for other capital structures;\n-   all entities listed on an exchange;\n-   all entities that trade stock or debt, investment vehicles, including mutual funds, pension funds and alternative investment vehicles constituted as corporate entities or collective investment agreements (including umbrella funds as well as funds under an umbrella structure, hedge funds, private equity funds, etc.);\n-   all entities under the purview of a financial regulator and their affiliates, subsidiaries and holding companies;\n-   counterparties to financial transactions."^^xsd:string ;
        skos:scopeNote               "The term 'legal entities' includes, but is not limited to, unique parties that are legally or financially responsible for the performance of financial transactions or have the legal right in their jurisdiction to enter independently into legal contracts, regardless of whether they are incorporated or constituted in some other way (e.g. trust, partnership, contractual). It excludes natural persons, but includes governmental organizations and supranationals."^^xsd:string .
