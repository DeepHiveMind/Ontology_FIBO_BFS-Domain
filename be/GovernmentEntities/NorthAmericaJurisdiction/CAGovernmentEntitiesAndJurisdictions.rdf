	<fibo-be-ge-ge:FederatedSovereignty
		rdf:about="&fibo-be-ge-caj;CanadianEntity">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Canadian entity</rdfs:label>
		<fibo-be-ge-ge:hasFullSovereigntyOver
			rdf:resource="&lcc-3166-1;Canada"/>
		<fibo-be-ge-ge:hasFullSovereigntyOver
			rdf:resource="&lcc-3166-2;NorthwestTerritories"/>
		<fibo-be-ge-ge:hasFullSovereigntyOver
			rdf:resource="&lcc-3166-2;Nunavut"/>
		<fibo-be-ge-ge:hasFullSovereigntyOver
			rdf:resource="&lcc-3166-2;YukonTerritory"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Alberta"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;BritishColumbia"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Manitoba"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;NewBrunswick"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;NewfoundlandAndLabrador"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;NovaScotia"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Ontario"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;PrinceEdwardIsland"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Quebec"/>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Saskatchewan"/>
		<fibo-be-ge-ge:isRepresentedBy
			rdf:resource="&fibo-be-ge-caj;GovernmentOfCanada"/>
		<rdfs:seeAlso
			rdf:resource="https://www.canada.ca/en/index.html"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the federated sovereignty and polity that is Canada</skos:definition>
	</fibo-be-ge-ge:FederatedSovereignty>
	
	<fibo-fnd-law-jur:Jurisdiction
		rdf:about="&fibo-be-ge-caj;CanadianJurisdiction">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Canadian jurisdiction</rdfs:label>
		<fibo-be-ge-ge:isJurisdictionOf
			rdf:resource="&fibo-be-ge-caj;GovernmentOfCanada"/>
		<fibo-fnd-law-jur:hasReach
			rdf:resource="&lcc-3166-1;Canada"/>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Canada#Government_and_politics</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Canada&apos;s federal structure divides government responsibilities between the federal government and the ten provinces. Provincial legislatures are unicameral and operate in parliamentary fashion similar to the House of Commons. Canada&apos;s three territories also have legislatures, but these are not sovereign and have fewer constitutional responsibilities than the provinces. The territorial legislatures also differ structurally from their provincial counterparts.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:seeAlso
			rdf:resource="http://www.justice.gc.ca/eng/csj-sjc/just/"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the overall jurisdiction for Canada</skos:definition>
	</fibo-fnd-law-jur:Jurisdiction>
	
	<fibo-be-ge-ge:RegionalGovernment
		rdf:about="&fibo-be-ge-caj;GovernmentOfAlberta">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>government of Alberta</rdfs:label>
		<fibo-be-ge-ge:hasJurisdiction
			rdf:resource="&fibo-be-ge-caj;ProvinceOfAlbertaJurisdiction"/>
		<fibo-fnd-rel-rel:governs
			rdf:resource="&lcc-3166-2;Alberta"/>
		<rdfs:seeAlso
			rdf:resource="http://www.alberta.ca/organizational-structure.cfm"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the regional government of the province of Alberta</skos:definition>
	</fibo-be-ge-ge:RegionalGovernment>
	
	<fibo-be-ge-ge:FederalGovernment
		rdf:about="&fibo-be-ge-caj;GovernmentOfCanada">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Government of Canada</rdfs:label>
		<fibo-be-ge-ge:hasJurisdiction
			rdf:resource="&fibo-be-ge-caj;CanadianJurisdiction"/>
		<fibo-fnd-rel-rel:governs
			rdf:resource="&lcc-3166-1;Canada"/>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Canada</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Canada is a federal parliamentary democracy and a constitutional monarchy, with Queen Elizabeth II being the head of state. The country is officially bilingual at the federal level. It is one of the world&apos;s most ethnically diverse and multicultural nations, the product of large-scale immigration from many countries. Its advanced economy is the eleventh largest in the world, relying chiefly upon its abundant natural resources and well-developed international trade networks.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:seeAlso
			rdf:resource="https://www.canada.ca/en/index.html"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the federal parliamentary democracy and constitutional monarchy of Canada</skos:definition>
	</fibo-be-ge-ge:FederalGovernment>
	
	<fibo-be-ge-ge:RegionalSovereignty
		rdf:about="&fibo-be-ge-caj;ProvinceOfAlbertaEntity">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Province of Alberta entity</rdfs:label>
		<fibo-be-ge-ge:hasSharedSovereigntyOver
			rdf:resource="&lcc-3166-2;Alberta"/>
		<fibo-be-ge-ge:isRepresentedBy
			rdf:resource="&fibo-be-ge-caj;GovernmentOfAlberta"/>
		<rdfs:seeAlso
			rdf:resource="http://www.alberta.ca/index.cfm"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the regional sovereignty and polity that is the Canadian Province of Alberta</skos:definition>
	</fibo-be-ge-ge:RegionalSovereignty>
	
	<fibo-fnd-law-jur:Jurisdiction
		rdf:about="&fibo-be-ge-caj;ProvinceOfAlbertaJurisdiction">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>province of Alberta jurisdiction</rdfs:label>
		<fibo-be-ge-ge:isJurisdictionOf
			rdf:resource="&fibo-be-ge-caj;GovernmentOfAlberta"/>
		<fibo-fnd-law-jur:hasReach
			rdf:resource="&lcc-3166-2;Alberta"/>
		<rdfs:seeAlso
			rdf:resource="https://albertacourts.ca/"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the overall jurisdiction for the Canadian Province Of Alberta, i.e., that of the Alberta Court of Appeal, Court of Queen&apos;s Bench, and Provincial Court of Alberta</skos:definition>
	</fibo-fnd-law-jur:Jurisdiction>

