    xmlns:fibo-fnd-plcx-re="https://spec.edmcouncil.org/fibo/FND/PlacesExt/RealEstate/"
    xmlns:fibo-loan-loan-col="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/"
