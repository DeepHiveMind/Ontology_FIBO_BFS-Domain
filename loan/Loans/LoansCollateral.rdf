    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/PlacesExt/RealEstate/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/MortgageLoans/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >LoansCollateral</rdfs:label>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/SecuredLoans/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansEvents/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateralAgreement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Contract in which the borrower of a collateralized securied loan agrees the terms on which the collateral is made available to the Lender.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >loan collateral agreement  (please review)</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/WrittenContract"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Assets pledged to a lender until a loan is repaid. Further notes: This provides one means by which some extension of finance (such as a loan, debt or equity issue) is under-written. PoC equivalent term definition and note: Collateral is property collateral or additional collateral associated with a loan or mortgage loan (PoC data note) There are foreign key attributes of collateral coupling it to collateral provider and to loan.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >loan collateral  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/SecuredLoans/CollateralizedSecuredLoan"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/supports"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/CollateralProvider"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/pledgedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Collateral"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Property Assets pledged to a lender until a loan is repaid. Further notes: This provides one means by which some extension of finance (such as a loan, debt or equity issue) is under-written.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >loan property collateral  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/PlacesExt/RealEstate/RealEstate"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/lien"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/MortgageLoans/MortgageLoan"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/MortgageLoans/isLienOn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/PropertyAsset"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/takesFormOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/RealEstate">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >real estate</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/isLien">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/RealEstate"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is lien  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/collateralValue">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >collateral value  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Additional Collateral value - Value of additional collateral</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/hasValuation">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/isOf"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansEvents/PropertyCollateralValuation"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has valuation  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/providedBy">
    <rdfs:label>provided by</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/supports">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Relationship introduced in PoC work, as inverse of another relationship already in model ("collateraized by").</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A loan which is supported by the Collateral</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/SecuredLoans/CollateralizedSecuredLoan"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >supports  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/isOf">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansEvents/PropertyCollateralValuation"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is of  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/propertyCollateralDSCRate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >property collateral d s c rate  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Debt Service Coverage Rate (DSC)</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/propertyCollateralTransferable">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >property collateral transferable  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Is property Transferability limited? For Spanish VPO loans, whether the property transferability is limited.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/pledgedBy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/CollateralProvider"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >pledged by  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/lien">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/PlacesExt/RealEstate/RealEstate"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >represents lien on  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/propertyCollateralLienSeniority">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Seniority on liquidation of property.</skos:definition>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >property collateral lien seniority  (please review)</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/collateralType">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Type of additional collateral.</skos:definition>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >collateral type  (please review)</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/propertyCollateralVPODeclassificationDate">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Time Until Declassification - For Spanish VPO loans, time (in months) until property will be declassified as VPO property Further notes: PoC SDM Data note: Converted to date for declassification to avoid dirty data (AO )</skos:definition>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >property collateral v p o declassification date  (please review)</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

