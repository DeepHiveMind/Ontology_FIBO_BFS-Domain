  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanInterestPaymentTermsSet">
    <skos:editorialNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Contains similar terms to debt securities interest payment terms, but defined separately here.</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Contractual terms governing the payment of interest on the loan.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >loan interest payment terms</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/IND/InterestRates/InterestRates/ReferenceInterestRate"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/hasBaseRate.1"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/loanInterestRateCap"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/InterestPaymentTerms"/>
