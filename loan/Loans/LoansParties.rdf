  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/hasCoBorower">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/Cosigner"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanContract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has co borower  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/hasLoanThirdParty">
    <rdfs:label>has loan third party</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/mayBe">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansGuaranties/LoanGuarantor"/>
