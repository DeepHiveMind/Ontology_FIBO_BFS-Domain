    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/hasIdentity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/originatedBy">
