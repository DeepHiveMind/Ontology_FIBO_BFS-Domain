bubu<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">is about loan collateral  (please review)</rdfs:label>
bubu<rdfs:domain rdf:resource="&fibo-loan-tem-col;CollateralDatedFacts"/>
