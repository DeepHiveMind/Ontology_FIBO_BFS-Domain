        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansTemporal/LoanRateDetermination">
