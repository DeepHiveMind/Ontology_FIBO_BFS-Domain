  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/ConstructionLoansTemporal/ConstructionLoanSnapshot">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This covers the "Loan advance to date" amount, which will have a current value on any given day and which will vary over time, as milestones are passed and additional advances drawn down.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Information at a point in time, specific to a Construction Loan.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >construction loan snapshot</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/ConstructionLoansTemporal/loanAdvanceToDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
