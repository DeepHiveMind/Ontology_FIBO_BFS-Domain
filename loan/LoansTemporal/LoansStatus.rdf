    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanArrearsStatus"/>
    <rdfs:label xml:lang="en">loan interest only temporary provision  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/loanPaymentInformationDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanPayment"/>
    <rdfs:label xml:lang="en">loan payment information date  (please review)</rdfs:label>
    <skos:definition xml:lang="en">The actual date on which the payment was made.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/paymentIdentifier">
    <skos:definition xml:lang="en">A unique identifier which identifies which scheduled payment this payment is made against.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TextValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanPayment"/>
    <rdfs:label xml:lang="en">payment identifier  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/informationDate.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
    <rdfs:label xml:lang="en">information date  (please review)</rdfs:label>
    <skos:definition xml:lang="en">The date on which these facts are or were applicable.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/studentLoanStatus">
    <skos:definition xml:lang="en">The status of the Student Loan at a given point in time, during the phase where it is active (the disbursement phase).</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/StudentLoanActiveStatusSelection"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/StudentLoanActiveStatus"/>
    <rdfs:label xml:lang="en">student loan status  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/currentLoanRate">
    <skos:definition xml:lang="en">The rate payable on the loan at this point in time.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
