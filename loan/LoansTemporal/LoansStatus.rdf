    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/DecimalValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
    <rdfs:label xml:lang="en">loan debt to income ratio  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/delinquent">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanArrearsStatus"/>
    <rdfs:label xml:lang="en">delinquent  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/anticipatedMaturityDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
    <rdfs:label xml:lang="en">anticipated maturity date  (please review)</rdfs:label>
    <skos:definition xml:lang="en">The date when the loan is expected to mature.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansStatus/hasSnapshot">
