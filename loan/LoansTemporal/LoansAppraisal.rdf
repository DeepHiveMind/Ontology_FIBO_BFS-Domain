<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-loan-tem-apr="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-fnd-aap-ppl="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/People/"
    xmlns:fibo-loan-tem-app="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanApplicationsTemporal/"
    xmlns:fibo-loan-loan-pty="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanApplicationsTemporal/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/People/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >LoansAppraisal</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/RepossessionHistory">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Information about an entity which has a history of repossessions, about those repossessions.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >repossession history  (please review)</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/MortgageLoanBorrowerAssessmentInformation">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >mortgage loan borrower assessment information  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanApplicationsTemporal/MortgageLoanApplication"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanBorrowersTemporal/isAbout.1"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanApplicationsTemporal/MortgageLoanAppraiser"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanBorrowersTemporal/appraisedBy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoanApplicationsTemporal/LoanApplicationBorrowerAssessmentInformation"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/appraisedBy">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/LoanServicer.1"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >appraised by  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/firstTimeBuyer">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/MortgageLoanBorrowerAssessmentInformation"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >first time buyer  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Whether the borrower is a first time buyer.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/mayBecome">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansParties/LoanBorrower"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >may become  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansAppraisal/obtainedFrom">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/People/Person"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >obtained from  (please review)</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

