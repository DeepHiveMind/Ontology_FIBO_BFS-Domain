  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/LoanProducts/LoanProductDisclosureRight">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Right to fair representation of the facts about Loan products before entrering into agreements for same.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >loan product disclosure right  (please review)</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansRegulatory/ProductDisclosureRight"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/LoanProducts/MortgageProduct">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >mortgage product  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoansCollateral/LoanPropertyCollateral"/>
