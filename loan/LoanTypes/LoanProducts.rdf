    >specifies  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/LoanProducts/isAbout">
    <rdfs:label>is about</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/LoanProducts/hasPreconditions">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CreditFacilities/LoanPrecedentConditions"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/LoanProducts/LoanProduct"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has preconditions  (please review)</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

