<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-loan-typ-gen="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/"
    xmlns:fibo-loan-loan-loan="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >GeneralLoans</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/AutoLoan">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A loan specifically for the purpose of automobile purchase.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >auto loan  (please review)</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanDrawdown"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinanceContract">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Contract embodying the terms under which a loan is advanced for marine finance.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >marine finance contract  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/setsOutTermsFor"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanContract"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/AutoLoanContract">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >auto loan contract  (please review)</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/AutoLoan"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/setsOutTermsFor"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanContract"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A loan for the purchase of a boat or other vessel.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >marine finance  (please review)</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanDrawdown"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/boatHeldAsChattel">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >boat held as chattel  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Whether the vessel is to be held in ownership as a form of chattel by the lender during the period of the loan.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/residential">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >residential  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Whether the boat is intended to be used and is legally able to be used as a place of residence.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/percentageAdvanced">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >percentage advanced  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The percentage of the purchase price advanced as the loan.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/mooredAt">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TextValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoanTypes/GeneralLoans/MarineFinance"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >moored at  (please review)</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Textual description (name etc.) of where the vessel is to be moored if it has a permanent mooring place.</skos:definition>
  </owl:ObjectProperty>
</rdf:RDF>

