	<owl:DatatypeProperty rdf:about="&fibo-ln-ln-up;rel-hasVersion">
		<rdfs:range rdf:resource="&xs;string"/>
		<skos:definition rdf:datatype="&xs;string">Indicates the version for something.</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-isHadBy">
		<owl:inverseOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<skos:definition rdf:datatype="&xs;string">Inverse of fibo-fnd-rel-rel:has</skos:definition>
		<skos:editorialNote rdf:datatype="&xs;string"></skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-isInformedBy">
		<skos:definition rdf:datatype="&xs;string">Relates, e.g. a  valuation to something that is  used to inform the valuation.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-isProducedBy_I">
		<owl:inverseOf rdf:resource="&fibo-ln-ln-up;rel-produces_I"/>
		<skos:definition rdf:datatype="&xs;string">Indicates who or what produced something.</skos:definition>
		<skos:editorialNote rdf:datatype="&xs;string">Ideally, this would be a superproperty of pas:isProducedBy in products and services ontology, but that is reserved for use with ThingInRole.  This property is therefore a superproperty of an unnamed property chain from the thing produced  to the independent producing thing that goes through an instance of a class that means PartyInProducerRole.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-isSubmittedBy">
		<rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition rdf:datatype="&xs;string">Relates a Report to the Agent that is providing the Report</skos:definition>
		<skos:editorialNote rdf:datatype="&xs;string">Ideally, this would be a subproperty of isProvidedBy, but that is reserved for use with ThingInRole.  This property is therefore a subproperty of an unnamed property chain from the thing submitted to the submitting  institution that goes through an instance of a class that means PartyInProviderRole.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-isSubmittedTo">
		<rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition rdf:datatype="&xs;string">Relates a Report to the Agent that is receiving the Report</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-produces_I">
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;rel-to">
		<skos:definition rdf:datatype="&xs;string">Relates something to a target or destination., e.g. the agent receiving a request.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-up;thingBeingValued">
		<rdfs:subPropertyOf rdf:resource="&fibo-ln-ln-up;hasParticipant"/>
		<skos:definition rdf:datatype="&xs;string">Relates a Valuatoin to the thing being valued.</skos:definition>
	</owl:ObjectProperty>

