	<owl:Class rdf:about="&fibo-ln-ln-up;ActualPayment">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-dt-fd;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-ln-ln-up;cur-hasAmount"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<skos:definition rdf:datatype="&xs;string">An actual payment occcurrence, whereby the borrower submits payment in order to fulfill an obligation to repay a loan.</skos:definition>
		<skos:editorialNote rdf:datatype="&xs;string">Topic: Payments</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-up;Application">
		<rdfs:subClassOf rdf:resource="&fibo-ln-ln-up;fnd-Request"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-ln-ln-up;hasDateReceived"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-dt-fd;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<skos:definition rdf:datatype="&xs;string">A request submitted by an applicant to receive something (e.g. a licence, a loan, a membership).  Is accompanied by documentation of pertinent information to determine whether the request should be granted.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-up;BE-ContactPurpose">
