		<skos:definition rdf:datatype="&xs;string">the purpose for contacting a person or organization</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-up;ChargeCategory">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-arr-cls;Classifier"/>
		<skos:definition rdf:datatype="&xs;string">A specific indicator of what a charge is for (e.g. closing costs, interest)</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-up;ContactInfo">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;LegallyCapableAdult"/>
