	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fbc-dae-gty;ContractGuarantor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractThirdParty"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty
							rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
						<owl:someValuesFrom
							rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>contract guarantor</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-agr-ctr;ContractParty"/>
		<skos:definition
			rdf:datatype="&xsd;string">a party that guarantees to underwrite some cash flow obligation on behalf of some other party</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-dae-gty;Guaranty">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fbc-dae-gty;ContractGuarantor"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-dae-gty;isGuaranteedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fbc-dae-gty;PriorityLevel"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-dae-gty;hasPriorityLevel"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-dae-gty;hasGuaranteedAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;Date"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arr-doc;hasExpirationDate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-bd;holdsDuring"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>guaranty</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an arrangement whereby cash flows on a debt security, e.g., interest payment, are guaranteed by a firm other than the issuer</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">can&apos;t this apply to any debt instrument, not just a security?  does this only apply to debt instruments that are securities, in other words?  we can say this via restrictions ...</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-dae-gty;PriorityLevel">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractualElement"/>
		<rdfs:label>priority level</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">the level of priority that guaranty has in the context of the contract, for example for a credit enhancement priority</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-dae-gty;hasGuaranteedAmount">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has guaranteed amount</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-dae-gty;Guaranty"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition
			rdf:datatype="&xsd;string">relates the guaranty to the monetary amount guaranteed</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-dae-gty;hasGuarantor">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasThirdParty"/>
		<rdfs:label>has guarantor</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-gty;ContractGuarantor"/>
		<skos:definition
			rdf:datatype="&xsd;string">relates the guarantor to the contract for which they are providing a guaranty</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-dae-gty;hasPriorityLevel">
		<rdfs:label>has priority level</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-dae-gty;Guaranty"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-gty;PriorityLevel"/>
		<skos:definition
			rdf:datatype="&xsd;string">relates a guaranty to some level of priority that guaranty has in the context of the contract, for example for a credit enhancement priority</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-dae-gty;isGuaranteedBy">
		<rdfs:label>is guaranteed by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-dae-gty;Guaranty"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-gty;ContractGuarantor"/>
		<skos:definition
			rdf:datatype="&xsd;string">relates guaranty to the contract guarantor, i.e., to the legal person providing the guaranty</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>