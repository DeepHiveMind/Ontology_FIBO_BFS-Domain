    <!-- Ontology/File-Level Metadata for the FIBO FBC Financial Products and Services Ontology -->

        <sm:filename rdf:datatype="&xsd;string">FinancialProductsAndServices.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fbc-pas-fpas</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FBC/20150801/ProductsAndServices/FinancialProductsAndServices/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology defines concepts that extend the Foundations (FND) Products and Services concepts specifically for the financial industry, including financial product, financial service, and financial service provider.</sm:fileAbstract>

