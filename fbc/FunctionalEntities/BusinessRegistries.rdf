	<owl:Ontology rdf:about="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/">
		<rdfs:label>Business Registries Ontology</rdfs:label>
		<dct:abstract>This ontology extends the Registration Authorities ontology to define specific kinds of registries, such as business entity registries, registries for identifiers and codes of various sorts, and registries for financial institutions and intermediaries based on jurisdiction, who regulates them, and the services they provide.</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2015-2017 EDM Council, Inc.
Copyright (c) 2015-2017 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/LCC/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/BE/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fbc-fct-breg</sm:fileAbbreviation>
		<sm:filename>BusinessRegistries.rdf</sm:filename>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FBC/AboutFBC/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/AboutFBCFunctionalEntities/"/>
		<owl:imports rdf:resource="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/Corporations/Corporations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/GovernmentEntities/GovernmentEntities/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/LEIEntities/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/FBC/20171201/FunctionalEntities/BusinessRegistries/"/>
		<skos:changeNote>The https://spec.edmcouncil.org/fibo/FBC/20150801/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FBC 1.1 RTF report.</skos:changeNote>
		<fibo-fnd-utl-av:hasMaturityLevel rdf:resource="&fibo-fnd-utl-av;Release"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&fibo-be-le-lp;LegalEntity">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasTransliteratedAddress"/>
				<owl:onClass>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
							</rdf:Description>
							<rdf:Description rdf:about="&fibo-be-le-fbo;RegisteredAddress">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:onClass>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasHeadquartersAddress"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
							</rdf:Description>
							<rdf:Description rdf:about="&fibo-be-le-fbo;RegisteredAddress">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistrationAuthority">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;issues"/>
				<owl:someValuesFrom rdf:resource="&fibo-be-corp-corp;RegistrationIdentifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;manages"/>
				<owl:someValuesFrom rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registration authority</rdfs:label>
		<skos:definition>a registration authority that is responsible for maintaining a registry of business entities</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>A business registry may include any government-managed registry for registering a business, such as a state department of corporations in the US, as well as other registries such as a local operating unit (LOU) for registration of legal entity identifiers (LEIs).  Any sanctioned registration authority as defined by the Registration Authorities List, published by GLEIF, is a business registration authority in this sense.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;Registry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
				<owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
				<owl:onClass rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasRegistryName"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registry</rdfs:label>
		<skos:definition>a registry for registering and maintaining information about business entities</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistryEntry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistryEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasRegistrationStatus"/>
				<owl:onDataRange rdf:resource="&xsd;string"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasEntityStatus"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasRegistrationStatusRevisionDate"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-dt-fd;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom rdf:resource="&fibo-be-corp-corp;RegistrationIdentifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registry entry</rdfs:label>
		<skos:definition>an entry in a business registry</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistryIdentifier">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-aap-agt;identifies"/>
				<owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registry identifier</rdfs:label>
		<skos:definition>an identifier that uniquely identifies a business registry, such as a registry identified by the Global Legal Entity Identifier Foundation (GLEIF) registration authorities register</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;LegalEntityIdentifierRegistry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
				<owl:onClass rdf:resource="&fibo-fbc-fct-breg;LocalOperatingUnit"/>
				<owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>legal entity identifier registry</rdfs:label>
		<skos:definition>a registry for registering and maintaining information about business entities for a particular jurisdiction</skos:definition>
		<fibo-fnd-utl-av:abbreviation>LEI registry</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;LegalEntityIdentifierRegistryEntry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-breg;BusinessRegistryEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasEntityExpiration"/>
				<owl:onClass rdf:resource="&fibo-fnd-dt-fd;DateTimeStamp"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasEntityCategory"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onDataRange rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasEntityExpirationReason"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onDataRange rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;hasBusinessRegistrationAuthority"/>
				<owl:someValuesFrom rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom rdf:resource="&fibo-be-le-lei;LegalEntityIdentifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>legal entity identifier registry entry</rdfs:label>
		<skos:definition>an entry in a legal entity identifier registry that conforms to ISO 17442 and the Global Legal Entity Identifier Foundation (GLEIF) Common Data Format (CDF)</skos:definition>
		<fibo-fnd-utl-av:abbreviation>LEI registry entry</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;LocalOperatingUnit">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;issues"/>
				<owl:someValuesFrom rdf:resource="&fibo-be-le-lei;LegalEntityIdentifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;manages"/>
				<owl:someValuesFrom rdf:resource="&fibo-fbc-fct-breg;LegalEntityIdentifierRegistry"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>local operating unit</rdfs:label>
		<skos:definition>a registration authority that issues legal entity identifiers and is authorized by the Global LEI Foundation to do so</skos:definition>
		<fibo-fnd-utl-av:abbreviation>LOU</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>LOUs supply registration, renewal and other services, and act as the primary interface for legal entities wishing to obtain an LEI.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;PhysicalAddress"/>
		<rdfs:label>registration address</rdfs:label>
		<skos:definition>an address for an organization used to register that organization in any registry, such as its legal address, headquarters address, a local address used to obtain a license or pay taxes in some jurisdiction, etc.</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine1">
		<rdfs:label>has address line 1</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the first line of the street address</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine2">
		<rdfs:label>has address line 2</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the second line of the street address</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine3">
		<rdfs:label>has address line 3</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the third line of the street address</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>This element SHALL be omitted if address line 2 is omitted.</fibo-fnd-utl-av:explanatoryNote>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine4">
		<rdfs:label>has address line 4</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the fourth line of the street address</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>This element SHALL be omitted if address line 3 is omitted.</fibo-fnd-utl-av:explanatoryNote>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasBusinessRegistrationAuthority">
		<rdfs:label>has business registration authority</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
		<skos:definition>identifies the business registration authority for the legal entity as defined in the GLEIF Registration Authorities List</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasCity">
		<rdfs:label>has city</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the name of the city</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasCountry">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has country</rdfs:label>
		<rdfs:range rdf:resource="&lcc-cr;Country"/>
		<skos:definition>identifies the country according to ISO 3166-1</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasDateDisclaimerIncluded">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDateValue"/>
		<rdfs:label>has date disclaimer included</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the date on which an IP disclaimer was included</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasEntityCategory">
		<rdfs:label>has entity category</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>indicates whether the entity fits a certain profile (i.e., is a branch, fund, or sole proprietorship) according to the GLEIF Common Data Format (CDF)</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasEntityExpiration">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDateTimeStamp"/>
		<rdfs:label>has entity expiration</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-dt-fd;DateTimeStamp"/>
		<skos:definition>the date and time on which an entity ceased to exist</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasEntityExpirationReason">
		<rdfs:label>has entity expiration reason</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>indicates the reason that an entity ceased to exist (i.e., disolved, merged with another entity, etc.)</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasEntityStatus">
		<rdfs:label>has entity status</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>indicates the status of the entity (i.e., active, inactive)</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasHeadquartersAddress">
		<rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasPrimaryAddress"/>
		<rdfs:label>has headquarters address</rdfs:label>
		<rdfs:range>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
					</rdf:Description>
					<rdf:Description rdf:about="&fibo-be-le-fbo;RegisteredAddress">
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:range>
		<skos:definition>identifies the headquarters address for the registered entity</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasInternationalOrganizationName">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasFormalName"/>
		<rdfs:label>has international organization name</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>an international name for the organization responsible for the register (i.e., in English)</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasLocalOrganizationName">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasFormalName"/>
		<rdfs:label>has local organization name</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>a local name for the organization responsible for the register</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasPostalCode">
		<rdfs:label>has postal code</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the postal code of this address as specified by the local postal service</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatus">
		<rdfs:label>has registration status</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the status of a specific registration, such as a registered identifier or license</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatusRevisionDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>has registration status revision date</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition>the date that the status of a specific registration in the registry was revised</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasRegistryName">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasName"/>
		<rdfs:label>has registry name</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-ra;Registry"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>a name for the registry, for example, for a business registry in which a business registration identifier for the legal entity is registered</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasRenewalDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>has renewal date</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition>the date by which a specific registration in the registry must be renewed or updated</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasSubdivision">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has subdivision</rdfs:label>
		<rdfs:range rdf:resource="&lcc-cr;CountrySubdivision"/>
		<skos:definition>identifies the country subdivision (state, province, region, etc.) according to ISO 3166-2</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasTransliteratedAddress">
		<rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasPrimaryAddress"/>
		<rdfs:label>has transliterated address</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<skos:definition>identifies a transliterated (i.e., in Latin or Romanized ASCII) address for the registered entity</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasValidationAuthority">
		<rdfs:label>has validation authority</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
		<skos:definition>identifies the business registration authority for the legal entity, used by the Local Operating Unit (LOU) as the basis for validation, as defined in the GLEIF Registration Authorities List</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasValidationLevel">
		<rdfs:label>has validation level</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>an indicator of the level of validation performed by the registrar with respect to the legal entity</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasWebsite">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-vrt;hasURL"/>
		<rdfs:label>has website</rdfs:label>
		<rdfs:range rdf:resource="&xsd;anyURI"/>
		<skos:definition>a website associated with the organization responsible for the register</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/gleif-registration-authorities-list</fibo-fnd-utl-av:adaptedFrom>
	</owl:DatatypeProperty>

</rdf:RDF>