	<owl:Ontology rdf:about="https://spec.edmcouncil.org/fibo/SEC/SecuritiesExt/BasketsExt/">
		<rdfs:label xml:lang="en">BasketsExt</rdfs:label>
		<owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
