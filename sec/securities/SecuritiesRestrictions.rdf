  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a security restriction applicable to holding the security</skos:definition>
    <rdfs:label>legal holding restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/QualifiedInvestorRestriction">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >If a holding period is not defined, then the period for which the restriction applies is indefinite.</fibo-fnd-utl-av:explanatoryNote>
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Needs review and provenance</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a legal holding restriction specifying that only qualified investors may hold the security, optionally for a defined holding period (if this is not defined then the period is indefinite)</skos:definition>
    <rdfs:label>qualified investor restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/hasHoldingPeriod"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/DatePeriod"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrument">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >extends the definition of a financial instrument to identify any relevant restrictions or registration requirements</fibo-fnd-utl-av:explanatoryNote>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/hasRestriction"/>
        </owl:onProperty>
        <owl:onClass>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
        </owl:onClass>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/hasRegistrationRequirement"/>
        </owl:onProperty>
        <owl:onClass>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/RegistrationRequirement"/>
        </owl:onClass>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/defines"/>
        <owl:onClass>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/ContractualRestriction"/>
        </owl:onClass>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/RegulationS">
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Needs review and provenance</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a securities regulation defining an exemption through which corporations can issue unregistered securities to qualified foreign investors and foreign institutions</skos:definition>
    <rdfs:label>Regulation S</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/QualifiedInvestorRestriction"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/InvestorsDomicileRestriction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/TradingRestriction">
    <rdfs:label>trading restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a legal restriction that is applicable to a financial instrument or security listing as mandated in a law or by contract</skos:definition>
    <rdfs:label>securities restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isMandatedBy"/>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/Law"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/Contract"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/appliesTo"/>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrument"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListings/SecurityListing"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/isInForceIn"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasGoverningJurisdiction"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/LegalConstruct"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/InvestorsDomicileRestriction">
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Needs review and provenance</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a legal holding restriction that specifies where holders of a security may be domiciled</skos:definition>
    <rdfs:label>investors' domicile restriction</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/IndividualInvestor">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an investor that is a person, rather than an organization, who may or may not be considered a qualified investor from a legal perspective</skos:definition>
    <rdfs:label>individual investor</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/hasIdentity"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/People/Person"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/OwnershipParties/Investor"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/BlueSkyLaw">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This allows investors to base their judgments on trustworthy data.</fibo-fnd-utl-av:explanatoryNote>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a kind of securities regulation passed by various states, designed to protect investors against securities fraud by requiring sellers of new issues to register their offerings and provide financial details</skos:definition>
    <rdfs:label>Blue-Sky law</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListings/SecurityListing">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >extends the definition of a security listing to identify any relevant restrictions</fibo-fnd-utl-av:explanatoryNote>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/hasRestriction"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/ContractualRestriction">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contract terms setting out restrictions on either the holder or the issuer of the security, as specified in the terms of the instrument itself</skos:definition>
    <rdfs:label>contractual restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isMandatedBy"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/Contract"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/ContractualCommitment"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/InstitutionalInvestor">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Typically more than 70 percent of the daily trading on the New York Stock Exchange is conducted on behalf of institutional investors.</fibo-fnd-utl-av:explanatoryNote>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014</fibo-fnd-utl-av:adaptedFrom>
    <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Example institutional investors include banks, insurance companies, mutual funds, pension funds, and other similar large funds.</skos:example>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an organization that trades large volumes of securities</skos:definition>
    <rdfs:label>institutional investor</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/OwnershipParties/Investor"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalRestriction">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a securities restriction that is mandated by law or regulation</skos:definition>
    <rdfs:label>legal restriction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isMandatedBy"/>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/Law"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isDefinedBy"/>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/Law"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a regulation codified in law specific to securities and investments</skos:definition>
    <rdfs:label>securities regulation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/defines"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRestriction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/isInForceIn"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasGoverningJurisdiction"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegulatoryAgencies/Regulation"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/RegistrationRequirement">
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Needs review and provenance</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a requirement for registraton of a security within some country, jurisdiction or federal province  ... applies to a registered security, is a function of registration regulations</skos:definition>
    <rdfs:label>registration requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/appliesTo"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/SecuritiesRegulation"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/Restriction144A">
