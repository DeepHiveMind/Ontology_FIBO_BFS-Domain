				<owl:onClass
					rdf:resource="&fibo-fnd-dt-fd;Date"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-fd;hasStartDate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
