		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-bd;BusinessRecurrenceIntervalConvention"/>
		<rdfs:label>day of month</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a business recurrence interval that indicates a specific, recurring day of the month</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This class is only used or meaningful when the regular period in the schedule is monthly, i.e., Period is 1 Month.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-isda;DayOfWeek">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-bd;BusinessRecurrenceIntervalConvention"/>
		<rdfs:label>day of week</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a business recurrence interval that indicates a specific, recurring day of the week</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This class is only used or meaningful when the regular period in the schedule is weekly, i.e., Period is 1 Week.</skos:scopeNote>
	</owl:Class>
	
