								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:datatype="&xsd;string">Commodity Business</rdf:first>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:datatype="&xsd;string">Currency Business</rdf:first>
												<rdf:rest>
													<rdf:Description>
														<rdf:first
															rdf:datatype="&xsd;string">Exchange Business</rdf:first>
														<rdf:rest>
															<rdf:Description>
