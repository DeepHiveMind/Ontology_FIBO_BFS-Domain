    <!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/" >
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/FND/Relations/Relations/" >
    <!ENTITY fibo-fnd-arr-arr "http://spec.edmcouncil.org/fibo/FND/Arrangements/Arrangements/" >
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/" >
    <!ENTITY fibo-fnd-acc-aeq "http://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/" >
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/" >
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/" >

    <!ENTITY fibo-be-oac-opty "http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/OwnershipParties/" >

    <!ENTITY fibo-fbc-fi-fi "http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/" >
    <!ENTITY fibo-fbc-pas-fpas "http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/" >

    <!ENTITY fibo-sec-sec-ast "http://spec.edmcouncil.org/fibo/SEC/Securities/SecurityAssets/" >
]>

<rdf:RDF xml:base="http://spec.edmcouncil.org/fibo/SEC/Securities/SecurityAssets/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
     xmlns:fibo-fnd-arr-arr="http://spec.edmcouncil.org/fibo/FND/Arrangements/Arrangements/"
     xmlns:fibo-fnd-oac-own="http://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/"
     xmlns:fibo-fnd-acc-aeq="http://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/"
     xmlns:fibo-fnd-acc-cur="http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
     xmlns:fibo-fnd-agr-ctr="http://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"

     xmlns:fibo-be-oac-opty="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/OwnershipParties/"

     xmlns:fibo-fbc-fi-fi="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"
     xmlns:fibo-fbc-pas-fpas="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"

     xmlns:fibo-sec-sec-ast="http://spec.edmcouncil.org/fibo/SEC/Securities/SecurityAssets/">


    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/SEC/Securities/SecurityAssets/">
        <rdfs:label>Security Assets Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO SEC Security Assets Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2016 EDM Council, Inc.
Copyright (c) 2016 Object Management Group, Inc.</sm:copyright>
