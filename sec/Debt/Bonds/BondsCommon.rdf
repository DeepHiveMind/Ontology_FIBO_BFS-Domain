    <rdfs:label xml:lang="en">equity linked bond</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/VariableCouponBond"/>
