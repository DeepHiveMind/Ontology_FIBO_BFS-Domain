        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowTerms/IndexLinkedPrincipalDeterminationTermsSet"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
