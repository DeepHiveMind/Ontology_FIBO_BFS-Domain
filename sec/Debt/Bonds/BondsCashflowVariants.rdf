        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/hasInterestPaymentTerms"/>
