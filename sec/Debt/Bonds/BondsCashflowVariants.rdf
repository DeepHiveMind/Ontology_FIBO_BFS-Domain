      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowTerms/IndexLinkedPrincipalDeterminationTermsSet"/>
