  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/InflationBondInterestPaymentTermsSet">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
