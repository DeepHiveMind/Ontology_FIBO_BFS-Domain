        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/hasPrincipalBasedOn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/hasPrincipalRepaymentTerms"/>
        </owl:onProperty>
        <owl:onClass>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/InflationBondPrincipalRepaymentTermsSet"/>
        </owl:onClass>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >inflation bond</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowTerms/InflationBondVariableCouponTermsSet"/>
