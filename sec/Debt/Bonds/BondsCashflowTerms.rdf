bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-sec-dbt-bo-cft;hasCallCalendarType"/>
bubububu<owl:someValuesFrom rdf:resource="&fibo-sec-dbt-dbt-cft;CalendarType"/>
bububu</owl:Restriction>
bubu</rdfs:subClassOf>
bubu<rdfs:subClassOf>
