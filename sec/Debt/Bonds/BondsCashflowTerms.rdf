bubu<rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bo-cft;CallSchedule"/>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-sec-dbt-bo-cft;schedulesPossibleEvent"/>
