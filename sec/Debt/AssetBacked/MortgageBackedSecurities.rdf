          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/responsibleFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/Actor"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/PassThroughMBSIssueUnderwriter">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The party which agrees to buy any certificates that are not bought by investors Term origin:MBS PoC Reviews</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >pass through m b s issue underwriter</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtIssuanceTerms/DebtIssueUnderwriter"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/GNMA-PlatinumPool">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >GNMA Platinum pool</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/AgencyMortgagePool"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/TrancheNonPayment">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Failure of a tranche of a tranched security to pay out. This may or may not be interpreted as a credit event.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >tranche non payment</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansEvents/NonPayment"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/FNMA-Pool">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
