  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/MBSPool">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A pool investment consisting of a collection of MBS instruments.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >m b s pool</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/CashStructuredFinanceInstrumentPool"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/CommercialMBS">
    <owl:disjointWith>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/ResidentialMBS"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >commercial m b s</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/MortgageBackedSecurityInstrument"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/SecuritizedMortgagePool">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This is a logical union of a non agency mortgage pool which is securitized and an agency mortgage pool which has been securitized.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A mortgage pool which is securitized as part of the issue of some mortgage backed security.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >securitized mortgage pool</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/NonAgencyIOTranche">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Interest Only tranche, meaning that this tranche will only pay interest.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >non agency i o tranche</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/TranchedMBSInstrument"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/SuperFloaterTranche">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >super floater tranche</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/FloaterTranche"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/AgencyMBSPool">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This pool may be used as the underlying for an agency CMO. Non agency CMOs do not exist.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A pool investment consisting of a collection of Agency MBS instruments.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >agency m b s pool</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/MBSPool"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/MortgageBackedSecurities/PayingAgent">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >paying agent</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/LoansTemporal/LoansEvents/Payment"/>
