    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/LoanPoolConstituent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >balance at inclusion date</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/hasConstituent.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/HomeEquityLineOfCreditPoolConstituent"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/HomeEquityLineOfCreditPool"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has constituent</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/hasConsituent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/poolHasLoanType">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >The type of loans that make up the pool.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/LOAN/Loans/LoanBasicTerms/LoanPurposeSelection"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/LoanPool"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >pool has loan type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/describedAs">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSPVs/DebtSPV"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/AssetBackedSecurities/ABSIssuer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >described as</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/BP/SecuritiesIssuance/IssuanceProcessTerms/securityIssuerDescribedAs"/>
  </owl:ObjectProperty>
</rdf:RDF>

