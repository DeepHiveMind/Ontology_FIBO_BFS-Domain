bububu</owl:Restriction>
bubu</rdfs:subClassOf>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
bubububu<owl:onProperty rdf:resource="&fibo-sec-dbt-ab-pbs;isTrancheOf"/>
bubububu<owl:someValuesFrom rdf:resource="&fibo-sec-dbt-ab-pbs;PoolOfIndividualDebts"/>
bububu</owl:Restriction>
bubu</rdfs:subClassOf>
bubu<rdfs:subClassOf>
bububu<owl:Restriction>
