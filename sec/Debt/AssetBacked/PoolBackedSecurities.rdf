  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/StructuredFinanceInstrument">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >This may be either an invented product (synthetic) or based on a real underlying asset pool. Either way, the underlying is defined by the issuing bank or investment company. These are traded. This is based on tranches of underlying asset pools. These are defined by the issuing bank or investment company.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A security that has periodic principal payments with the amount of principal that is paid being dictated by a published factor.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >structured finance instrument</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/Bonds/BondsCashflowVariants/AmortizingSecurity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/mayBe"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/Asset"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/hasUnderlying.1"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/CashStructuredFinanceInstrumentPool">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A pool investment consisting of a collection of cash structured finance instruments.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >cash structured finance instrument pool</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/StructuredFinanceInstrumentPool"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/DebtCollateral">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Debt Assets pledged to a lender until a loan is repaid. Further notes: This provides one means by which some extension of finance (such as a loan, debt or equity issue) is under-written.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >debt collateral</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/FinancialAsset"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/takesFormOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Collateral"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/PoolBackedSecurities/CashStructuredFinanceInstrument">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A structured finance instrument with a real asset underlying.</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/AssetBacked/SyntheticCDOs/SyntheticStructuredFinanceInstrument"/>
