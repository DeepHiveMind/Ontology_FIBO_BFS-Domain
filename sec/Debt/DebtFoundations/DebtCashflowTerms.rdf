    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <rdfs:label>pay in kind</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/compoundingFrequency">
    <skos:definition>The frequency at which the interest is compounded. This is established at the initial stages of securing the instrument. Generally, interest tends to be calculated on an annual basis although other terms may be established at the time of the loan.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Frequency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <rdfs:label>compounding frequency</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasNotificationFeature">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/NotificationFeature"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>has notification feature</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/isLegallyDefeased">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <terms:description>whether the maturity is legally defeased</terms:description>
    <rdfs:label>is legally defeased</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/perpetualMaturity">
    <skos:definition>Whether the debt security has perpetual maturity, i.e. there is no Maturity Date and the security remains in issue indefinitely or until recalled.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>perpetual maturity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/putFrequency">
    <skos:definition>Frequency of Tender or Put period.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Frequency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>put frequency</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasAccrualBasis">
