    </rdfs:subClassOf>
    <rdfs:label>put schedule listing</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Schedule of dates on which a Traded Security may be Put by the Holder.</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/NonTradableDebtInterestTerms">
