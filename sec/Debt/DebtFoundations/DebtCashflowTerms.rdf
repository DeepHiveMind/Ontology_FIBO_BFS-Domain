    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Math/FormulaSubject"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/legalFinalMaturityDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <fibo-fnd-utl-av:explanatoryNote>The Legal Maturity Date is later than the expected maturity date and is the date on which the principal must be paid to investors. Also called final maturity date. It is also the termination or due date on which an installment loan must be paid in full.</fibo-fnd-utl-av:explanatoryNote>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>legal final maturity date</rdfs:label>
    <skos:definition>the date on which the principal must be paid to investors</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/roundingDecimalPlaces">
    <skos:definition>Number of decimal places to round to.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/Base"/>
    <rdfs:label>rounding decimal places</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/payInKind">
