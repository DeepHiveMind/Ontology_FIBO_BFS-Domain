    <rdfs:label>put date</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ScheduledEventDate"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/SecuritizedDebtInterest">
