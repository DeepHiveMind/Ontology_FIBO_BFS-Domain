    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Frequency of Tender or Put period.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasAccrualBasis">
