    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutOfIssue"/>
    <rdfs:label>occurs on</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/hasEventDate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/truncatingDecimalPlaces">
