    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Math/hasArgument"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/firstParCallPrice">
