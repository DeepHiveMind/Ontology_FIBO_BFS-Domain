    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/Margin"/>
    <rdfs:label>amount</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/occursOn.1">
