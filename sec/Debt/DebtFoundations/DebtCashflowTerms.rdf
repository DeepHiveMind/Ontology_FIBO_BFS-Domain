    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/NotificationFeature"/>
    <rdfs:label>notification</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasInterestTerms">
    <rdfs:label>has interest terms</rdfs:label>
  </owl:ObjectProperty>
