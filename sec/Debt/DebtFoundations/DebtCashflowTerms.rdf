    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/Base"/>
    <rdfs:label>truncating decimal places</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/mandatorySinkingFund">
    <skos:definition>Whether there is a Mandatory Sinking fund feature.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>mandatory sinking fund</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/firstPremiumCallDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>first premium call date</rdfs:label>
