      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Time/Time/BusinessDay"/>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/Time/Time/CalendarDay"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label>calendar day type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/InterestCalculationSubject">
    <skos:definition>The Subject or Left Hand Side of an Interest Calculation Formula. This is by definition the Interest amount.</skos:definition>
    <rdfs:label>interest calculation subject</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/SecuritizedDebtInterest"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/subjectIs"/>
        </owl:onProperty>
      </owl:Restriction>
