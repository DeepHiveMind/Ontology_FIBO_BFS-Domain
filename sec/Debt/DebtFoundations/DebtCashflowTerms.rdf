  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/Base">
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Math/FormulaArgument"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/roundingDecimalPlaces"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/truncatingDecimalPlaces"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Math/hasOperand"/>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/MarketRate"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label>base</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The variable to which an additional Margin amount is added to get the Interest amount.</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutScheduleListing">
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/AdHocSchedule"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutSchedule"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/listsScheduledEvent"/>
        <owl:someValuesFrom>
