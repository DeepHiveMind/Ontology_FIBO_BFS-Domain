    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>estate or death put</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/subjectIs">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/SecuritizedDebtInterest"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/InterestCalculationSubject"/>
    <rdfs:label>subject is</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/paymentInKind">
    <skos:definition>Whether the Principal may be repaid in kind rather than in cash.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>payment in kind</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/interestPaymentFrequency">
    <skos:definition>Frequency of payment of Interest on the debt.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Frequency"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <rdfs:label>interest payment frequency</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/firstPremiumCallPrice">
    <skos:definition>The price of the bonds on the first part call at a price above par.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/PercentageValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>first premium call price</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/firstParCallDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>first par call date</rdfs:label>
    <skos:definition>The first date on which bonds may be called at Par for redemption.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/extendableDate">
    <skos:definition>Date to which the Maturity Date may be extended. This date is determined at the set-up of the bond. Note this date is adjustable according to various applicable parameters or terms. May have a rolling extendable maturity date. The bond holder may choose to extend. Usually a 13 month rolling extendable maturity.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/DeterminedDate"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/ExtendableMaturityRedemptionTermsSet"/>
    <rdfs:label>extendable date</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/extendableByHolder">
    <skos:definition>Indicates whether the expiration date or maturity date can be extended by the holder, i.e. repayment may be extended or maturity changed.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>extendable by holder</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/includesWindow.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutWindow"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>includes window</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/daysToPutNotification">
    <skos:definition>Number of days prior to a mandatory or optional redemption date that the issuer/agent must give notice to holders. This term replaces the Multipler term in the Duratiojn Description.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutWindow"/>
    <rdfs:label>days to put notification</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/rightToRetain">
    <skos:definition>Whether the holder has an option to retain the security in the event of an issuer exercising the put option.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>right to retain</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/datedDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <terms:description>the date on which an interest paying debt instrument begins to accrue interest</terms:description>
    <rdfs:label>dated date</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/putDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Date"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>put date</rdfs:label>
    <skos:definition>Date on which securities are subject to Optional or Mandatory Redemption by the bond holder.</skos:definition>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasPrincipalAmortizationType">
    <skos:definition>The type of principal payments applicable to this security.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/AmortizationType"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/PrincipalRepaymentTerms"/>
    <rdfs:label>has principal amortization type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/autoReinvestment">
    <skos:definition>Whether the security provides for mandatorily &amp; automatically re-investing the interest from that security towards buying more of the same security.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <rdfs:label>auto reinvestment</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/extendableByIssuer">
    <skos:definition>Whether the issuer has the option to extend the debt rather than refinancing. If not, the issuer may only refinance the debt by calling the issue and creating a new issue.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>extendable by issuer</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/extraordinaryCallProvision">
    <skos:definition>Indicates the existence of an Extraordinary Call provision in the bond call terms.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtCallTerms"/>
    <rdfs:label>extraordinary call provision</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/includes">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutSchedule"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutTerms"/>
    <rdfs:label>includes</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasPutCalendarType">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/CalendarType"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/PutWindow"/>
    <rdfs:label>has put calendar type</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/hasCalendarDenomination"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/hasFormula">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/InterestCalculationFormula"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/DebtSecurityInterestPaymentTerms"/>
    <rdfs:label>has formula</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/isQualifiedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/includesForExtendableMaturity">
    <skos:definition>In the case of terms where extendable maturity is yes, terms are included regarding the extension of the maturity on the security.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/ExtendableMaturityRedemptionTermsSet"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/TradableDebtSecurityRedemptionTermsSet"/>
    <rdfs:label>includes for extendable maturity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/extendablePeriod">
    <skos:definition>Period during which the Maturity Date may be extended.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/DatePeriod"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/ExtendableMaturityRedemptionTermsSet"/>
    <rdfs:label>extendable period</rdfs:label>
  </owl:ObjectProperty>
</rdf:RDF>

