		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-sec-dbt-dbt-fin;collateralizedBy"/>
				<owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FND/AccountingExt/DebtTerms/Collateral"/>
