  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/ContractGuarantor">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A Party which guarantees to underwrite some cash flow obligation on behalf of some other party.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >contract guarantor</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/ContractThirdParty"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/Guarantor"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/Guaranty">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >guaranty</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/guarantyTakesFormOf"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Arrangement whereby cash flows on the debt security, e.g., interest payment, are guaranteed by a firm other than the issuer.</skos:definition>
  </owl:Class>
