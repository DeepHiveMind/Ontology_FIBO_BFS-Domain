  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/DebtInstrument">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A paper or electronic obligation that enables the issuing party to raise funds by promising to repay a lender in accordance with terms of a contract. Types of debt instruments include notes, bonds, certificates, mortgages, leases or other agreements between a lender and a borrower.</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/ParityVariants/DiscountedInstrument"/>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/ParityVariants/InstrumentIssuedAtPar"/>
          <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/ParityVariants/PremiumInstrument"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label>debt instrument</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/WrittenContract"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/poolNumber">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The Pool Number of the debt.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/IntegerValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/PubliclyIssuedDebt"/>
    <rdfs:label>pool number</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/callable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Whether the writer of the debt contract (the debtor) can call the debt prior to maturity.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/NonTradableDebtInstrument"/>
    <rdfs:label>callable</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/exchangeable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Whether the security can be exchanged for another security at the issuers discretion.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/TradableDebtInstrument"/>
    <rdfs:label>exchangeable</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/relatesTo">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/DebtFinance"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/DebtInstrument"/>
    <rdfs:label>relates to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/hasDebtGuarantor">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtIssuanceTerms/DebtGuarantor"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/TradableDebtInstrument"/>
