  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutNotificationProvision">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a provision of a put feature defining notification requirements</skos:definition>
    <rdfs:label>put notification provision</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/hasDatePeriod"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutWindow"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrumentRedemptionProvision"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PubliclyIssuedDebt">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an issued debt in the form of a tradable debt instrument (security)</skos:definition>
    <rdfs:label>publicly issued debt</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isConferredBy"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/IssuedDebt"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an offering of one or more debt instruments for sale</skos:definition>
    <rdfs:label>debt offering</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/PrincipalRepaymentTerms"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRepaymentTerms"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/InterestPaymentTerms"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasInterestPaymentTerms"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/appliesTo"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRedemptionProvision"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/RedemptionProvision"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasCallFeature"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallFeature"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesIssuance/SecuritiesOffering"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesIssuance/hasOffering"/>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallNotificationProvision">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a provision of a call feature defining notification requirements</skos:definition>
    <rdfs:label>call notification provision</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/hasDatePeriod"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallWindow"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrumentRedemptionProvision"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutFeature">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >put provision</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >FIBIM has term "Putable Date" which (by implication, and comparing with definition for "Next Call Date") is presumably a single calendar date in the future, at a given point in time. That does not cover the definition of formal terms defining when and how the issue may be put, which is what is modeled here.</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a redemption provision giving the holder the right, but not the obligation, to sell a specified amount of the debt instrument (i.e., redeem it), prior to maturity</skos:definition>
    <rdfs:label>put feature</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutSchedule"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutNotificationProvision"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrumentRedemptionProvision"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/NonTradableDebtRedemptionProvision">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >terms for repayment of principal on debt instruments that are not traded securities, such as over the counter, bilateral paper</skos:definition>
    <rdfs:label>non-tradable debt redemption terms</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/RedemptionProvision"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallPrice">
    <fibo-ind-ei-ei:actualExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >par value + call premium</fibo-ind-ei-ei:actualExpression>
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >redemption price</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a price at which a debt instrument with a call feature may be redeemed by the issuer</skos:definition>
    <rdfs:label>call price</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryPrice"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallEvent">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an event associated with the call schedule for a debt instrument</skos:definition>
    <rdfs:label>call event</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/OccurrenceKind"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallSchedule">
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a schedule that defines the events associated with the call feature of a debt instrument, i.e, the dates on which the debt instrument may be called (recalled) at what price by the issuer</skos:definition>
    <rdfs:label>call schedule</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallPremium"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallPrice"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallEvent"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Schedule"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutPremium">
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/p/put.asp</fibo-fnd-utl-av:adaptedFrom>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an amount over par that a debt instrument holder must pay to sell the security early</skos:definition>
    <rdfs:label>put premium</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryPrice"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutWindow">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an explicit period of time prior to a put date during which holder or agent must give notice to an issuer</skos:definition>
    <rdfs:label>put window</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/DatePeriod"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Details from Ecofin: A [debt] instrument can be traded, if its features depend only on one borrower. If the instrument has no bilateral or multilateral obligations, the investor can easily transfer it to another investor without asking the borrower (except the terms prohibit this explicitly). This is simplified with securitised instruments, where the debt is already split into handy denominations which trade easily (e.g. in round thousands or millions as with bonds, commercial paper, etc.). But in principle it works also with interbank loans and similar instruments. FIBIM Definition: Financial instruments evidencing moneys owed by the issuer to the holder on terms as specified.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a debt instrument that is also a security, i.e., that can be bought and sold by the holder</skos:definition>
    <rdfs:label>tradable debt instrument</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesIssuance/hasOffering"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/Guarantor"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/hasGuarantor"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/mayBeSubordinatedTo"/>
        </owl:onProperty>
        <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutPrice">
    <fibo-ind-ei-ei:actualExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >par value + put premium</fibo-ind-ei-ei:actualExpression>
    <skos:editorialNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >needs review - should the expression be exercise value vs. par value?</skos:editorialNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a price at which a debt instrument with a put feature may be sold by the holder</skos:definition>
    <rdfs:label>put price</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryPrice"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutEvent">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an event associated with the put schedule for a debt instrument, i.e., an event involving the 'put', or surrender of the instrument by the holder</skos:definition>
    <rdfs:label>put event</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/OccurrenceKind"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallWindow">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an explicit period of time prior to a call date during which an issuer or agent must give notice to holders</skos:definition>
    <rdfs:label>call window</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/DatePeriod"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallPremium">
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an amount over par that an issuer must pay an investor to redeem the security early</skos:definition>
    <rdfs:label>call premium</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryPrice"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/MakeWholeFeature">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >make whole provision</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The issuer typically has to make a lump sum payment to the investor derived from a formula based on the net present value (NPV) of future interest or coupon payments that will not be paid incrementally because of the call combined with the principal payment the investor would have received at maturity.</fibo-fnd-utl-av:explanatoryNote>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://www.investopedia.com/terms/m/make-wholecall.asp</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a call provision allowing the issuer to pay off remaining debt early</skos:definition>
    <rdfs:label>make whole feature</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallFeature"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutSchedule">
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/p/put.asp</fibo-fnd-utl-av:adaptedFrom>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a schedule that defines the events associated with the put feature of a debt instrument, i.e, the dates on which the debt instrument may be sold at what price by the holder</skos:definition>
    <rdfs:label>put schedule</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutPremium"/>
              <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutPrice"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PutEvent"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/Schedule"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/IssuedDebt">
    <rdfs:label>issued debt</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >debt that is set out by the borrower in some form of financial security in which the lender is the holder or counterparty of that security</skos:definition>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasDebtAmount"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryAmount"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/isConferredBy"/>
        <owl:allValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrumentRedemptionProvision">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Debt represented by a tradable debt security may be paid as a one off payment at maturity, by paying down principal amounts periodically, or by a call of the issue by the issuer. Debt that is defined by a convertible bond are redeemed by conversion into an equity security issued by the same company. More notes from the EDMC SME Review: Distinction between call and amortizing is whether you pay off everybody at the same time or via a lottery. SF can be either. either amortize the payment of a SF or pay it off by a lottery. The terms of the instrument determine that. Mandatory - all in the schedule. Mandatory to the bond issuer. Issuer has to pay off a certain amount at a certain time - this is in the Schedule. Callable - nothing mandatory.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >terms for the redemption of the debt represented by a debt security</skos:definition>
    <rdfs:label>tradable debt instrument redemption provision</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/RedemptionProvision"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/NonTradableDebtInstrument">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Generally, a nonnegotiable instrument may be redeemed by the issuer, but this is often subject to some limitations.</fibo-fnd-utl-av:explanatoryNote>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Non-Tradeable. (n.d.) Farlex Financial Dictionary. (2009). Retrieved December 16 2016 from http://financial-dictionary.thefreedictionary.com/Non-Tradeable</fibo-fnd-utl-av:adaptedFrom>
    <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Low-risk instruments such as savings bonds are examples of nonnegotiable debt instruments.</skos:example>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a debt instrument that may not be bought or sold</skos:definition>
    <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
    <rdfs:label>non-tradable debt instrument</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/MutualContractualAgreement"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallFeature">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >call provision</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Most corporate and municipal bonds have ten-year call features (termed call protection by holders); government securities typically have none.</fibo-fnd-utl-av:explanatoryNote>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a redemption provision defining the schedule and price of redemption prior to maturity</skos:definition>
    <rdfs:label>call feature</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallSchedule"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallNotificationProvision"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrumentRedemptionProvision"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/RelativePrice">
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/p/par.asp</fibo-fnd-utl-av:adaptedFrom>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/m/market-price.asp</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a relative price with respect to either a stated or market value for a debt instrument at some point in time, defined as par, premium, or discount</skos:definition>
    <rdfs:label>relative price</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/MonetaryPrice"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRelativePriceAtRedemption">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates a relative price with respect to the face value at the time an instrument is redeemed, namely par, discount or premium</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/RelativePrice"/>
    <rdfs:label>has value at redemption</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/hasPrice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasCallFeature">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/CallFeature"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>has call terms</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRepaymentTerms"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRelativePriceAtIssue">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates a relative price with respect to the face value at which an instrument is issued, namely par, premium or discount</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/RelativePrice"/>
    <rdfs:label>has value at issue</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/hasPrice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/mayBeSubordinatedTo">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/TradableDebtInstrument"/>
    <rdfs:label>may be subordinated to</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasInterestPaymentTerms">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/InterestPaymentTerms"/>
    <rdfs:label>has interest payment terms</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasContractualElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasPriceAndYieldDayCountConvention">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates the convention used to determine the number of days in a month and days in a year that are counted when performing calculations for yield and price figures</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DayCountConvention"/>
    <rdfs:label>has price and yield day count convention</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/BusinessDates/hasBusinessRecurrenceIntervalConvention"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRedemptionProvision">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/RedemptionProvision"/>
    <rdfs:label>has redemption terms</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasContractualElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasRepaymentTerms">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/PrincipalRepaymentTerms"/>
    <rdfs:label>has repayment terms</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/hasContractualElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasDurationToMaturity">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates the lifespan of a tradable debt instrument, from the issue date to the scheduled maturity date</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/ExplicitDuration"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>has duration to maturity</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/hasDuration"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasDefaultLotSize">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This is the minimum denomination required for transfer or change of ownership of a tradable debt security.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates the default number of units of the security that may be held at any one time</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>has default lot size</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/hasNumericValue"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/isExchangeable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates whether the security can be exchanged for another security at the issuers discretion</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>is exchangeable</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/isPutable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates whether the holder has the right to ask for redemption of the security prior to final maturity</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>is putable</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/hasPoolNumber">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates the pool number of the debt</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/PubliclyIssuedDebt"/>
    <rdfs:label>has pool number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/hasNumericValue"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/isCallable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates whether the issuer (writer) of the debt contract (the debtor) can call the debt prior to maturity</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>is callable</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/isSubordinated">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates whether the security is a subordinated security, meaning that the security has a lower priority than another security so that when the assets are liquidated this one is not first in line</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>is subordinated</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/isStrippable">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates whether or not the instrument can be stripped, so that interest and principal may be traded separately</skos:definition>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/DebtOffering"/>
    <rdfs:label>is strippable</rdfs:label>
  </owl:DatatypeProperty>
  <owl:NamedIndividual rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/AtAPremium">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >premium</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >above par</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/a/at-a-premium.asp</fibo-fnd-utl-av:adaptedFrom>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a selling price significantly above the stated face or redemption value due to high demand or timing of redemption</skos:definition>
    <rdfs:label>at a premium</rdfs:label>
    <rdf:type rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/RelativePrice"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/ParValue">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >par</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nominal value</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >face value</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.investopedia.com/terms/p/par.asp</fibo-fnd-utl-av:adaptedFrom>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the stated value of a negotiable instrument, stock, or bond, as compared with the value that instrument might receive when sold</skos:definition>
    <rdfs:label>par value</rdfs:label>
    <rdf:type rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/RelativePrice"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/AtADiscount">
    <fibo-fnd-utl-av:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >below par</fibo-fnd-utl-av:synonym>
    <fibo-fnd-utl-av:adaptedFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
