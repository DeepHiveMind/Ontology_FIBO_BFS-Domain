    <!-- Ontology-Level Metadata for About the EDMC-FIBO Securities (SEC) Domain Version 2.0, Reference Individuals -->

        <sm:filename rdf:datatype="&xsd;string">AboutSEC-2.0-ReferenceIndividuals.rdf</sm:filename>
