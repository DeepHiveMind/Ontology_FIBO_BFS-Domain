        <skos:definition rdf:datatype="&xsd;string">individual representing metadata about the FIBO Securities (SEC) Version 2.0 Domain</skos:definition>
        <sm:publicationDate rdf:datatype="&xsd;dateTime">2018-06-22T18:00:00</sm:publicationDate>
