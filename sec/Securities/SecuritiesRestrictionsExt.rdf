<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:fibo-fnd-lawx-rst="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"
    xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
    xmlns:fibo-fnd-law-jur="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:fibo-sec-sec-resx="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:fibo-bp-reg-req="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
    xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"/>
    <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >SecuritiesRestrictionsExt</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >security</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/holderRestrictions"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/contractuallyDefinesRestrictions"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityContractualRestriction"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRestriction">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/restrictionAppliesIn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >investment restriction</rdfs:label>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Restrictions applicable to a financial instrument as provided for in a body of Law.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/LegalRestriction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityContractualRestriction">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >These are restrictions defined in the contractual terms of the security itself and form part of the terms of the security, as distinct from restrictions defined in some body of law and being applicable to the security, which are defined separately.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Contractual terms setting out restrictions on either the holder or the issuer of the security.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >security contractual restriction</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/ContractualRestriction"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/FinancialInstrument">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >financial instrument</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityRegistrationRequirement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A Requirement for Registraton of a Security within some Country, Jurisdiction or Federal Province.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >security registration requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/appliesTo"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRegulatoryRequirement"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/RegistrationRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRegulatoryRequirement">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A requirement laid down in law for investment regulation.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >investment regulatory requirement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRegulatoryLaw"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/mandatedBy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/RegulatoryRequirement"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRegulatoryLaw">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A body of law dealing with investment regulation.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >investment regulatory law</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/StatuteLaw"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestorType">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >investor type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestorDomicileType">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >investor domicile type</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/hasEffectivePeriod">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Period during which the restriction applies.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/DatePeriod"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRestriction"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has effective period</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/holderRestrictions">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >holder restrictions</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/contractuallyDefinesRestrictions">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityContractualRestriction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >contractually defines restrictions</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/hasRegistrationRequirement">
    <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/appliesTo"/>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityRegistrationRequirement"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has registration requirement</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/hasRestrictions">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRestriction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/FinancialInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has restrictions  (please review)</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/hasRestriction">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/InvestmentRestriction"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >has restriction</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/isInclusive">
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >Whether the Restriction is defined inclusively (the stated terms put something within the Restriction), or Exclusively (the stated terms put something outside the reach or remit of the Restriction). Yes = Inclusive.</skos:definition>
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/TrueFalseValue"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/SecurityRegistrationRequirement"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >is inclusive</rdfs:label>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/Security"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrument">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/FinancialInstrument"/>
  </rdf:Description>
</rdf:RDF>

