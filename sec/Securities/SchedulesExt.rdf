          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/schedules"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ScheduledEvent"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ExplicitlyScheduledEvent">
    <fibo-fnd-utl-av:explanatoryNote rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >This is as distinct from an event the date for which is specified in terms of some formula or heuristic for determining that date.</fibo-fnd-utl-av:explanatoryNote>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >An event which is scheduled in advance to take place on some specific date.</skos:definition>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >explicitly scheduled event</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ScheduledEventDate"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/hasEventDate"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/OccurrenceKind"/>
  </owl:Class>
