  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/listsScheduledEvent">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ExplicitlyScheduledEvent"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/AdHocSschedule"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >lists scheduled event</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/comprises"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/hasEventDate">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ScheduledEventDate"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ExplicitlyScheduledEvent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >has event date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/hasDate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/schedules">
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString" xml:lang="en"
    >schedules</rdfs:label>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/RegularSchedule">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/RegularSchedule"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/AdHocSchedule">
    <owl:equivalentClass rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/AdHocSschedule"/>
  </rdf:Description>
