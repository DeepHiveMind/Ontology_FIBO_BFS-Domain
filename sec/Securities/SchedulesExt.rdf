    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/RegularSchedule"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/scheduledBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/Event"/>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/OccurrenceKind"/>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/ScheduledEventDate">
