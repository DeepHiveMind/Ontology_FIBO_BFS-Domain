    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/Security">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/SecurityStructuredName"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/hasStructuredName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >A financial instrument which can be bought or sold. This may be traded either over the counter or through an exchange or other trading venue.</skos:definition>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/BP/SecuritiesIssuance/IssuanceProcessTerms/hasIssuanceInformation"/>
