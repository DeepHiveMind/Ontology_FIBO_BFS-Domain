      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/SecuritySettlementConvention"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/hasDefaultSettlementTerms"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/Jurisdiction"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/legallyRecordedIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesTaxTreatment/SecurityTaxTreatment"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesTaxTreatment/hasTaxTreatment"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListings/FinancialMarket"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesListings/issuedIn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/Currency"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/denominationCurrency"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/TransferableContract"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/TemporalInfo/hasStatus"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/hasStructuredName"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/SecurityStructuredName"/>
